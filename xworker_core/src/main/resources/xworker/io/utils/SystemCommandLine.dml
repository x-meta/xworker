#$@text#$@
//初始化变量
def br = new BufferedReader(new InputStreamReader(System.in));
_g.put("br", br);
_g.put("out", System.out);

//打印欢迎信息
out.println(self.doAction("getMessage", actionContext));
//
def nodes = self.getChilds("SystemCommandLineNode");
if(nodes.size() == 0){
    return;
}else if(nodes.size() == 0){
    nodes.get(0).doAction("selected", actionContext);
}else{
    def index = 1;
    for(node in nodes){
        out.println(index + "\t" + node.metadata.label);
        index++;
    }
    out.print(self.doAction("getPrompt", actionContext));
    def line = br.readLine();
    def ok = false;
    try{
        int s = Integer.parseInt(line);
        if(s >=1 and s <= nodes.size()){
            ok = true;
        }
    }catch(Exception e){
    }
}
