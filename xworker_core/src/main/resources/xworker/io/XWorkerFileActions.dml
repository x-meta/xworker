#$@text#$@
//获取要写入的文件
def file = self.doAction("getFile", actionContext);

println("cpy thing manager:" + file + ", fileattr=" + self.getString("file"));
println(self.getMetadata().getPath());
//写入文件
self.doAction("writeToFile", actionContext, "file", file);
