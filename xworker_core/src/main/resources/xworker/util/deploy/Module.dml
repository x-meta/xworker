    Scode=
#$@text#$@
import xworker.util.deploy.ModuleManager;

import org.xmeta.Thing;

def mm = new ModuleManager();
mm.addModule(self);
mm.calculate();

for(lib in mm.getLibs()){
    println lib.getPath();
}

for(res in mm.getResources()){
    println res.getPath();
}
