def module = thing.getMetadata().getName();
def command = ["java",
    "-classpath","xworker.jar", "xworker.filesync.FileSync",
    module] as String[];

def dir = new File(world.getPath());
startProcess(actionContext, "dir", dir, "cmds", command, "title", "Update module " + module);

msg(actionContext);
