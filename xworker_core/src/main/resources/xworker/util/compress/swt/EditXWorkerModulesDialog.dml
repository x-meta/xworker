          Scode=
#$@text#$@
//选中的事物列表
def things = thingRegistor.doAction("getCheckedThings", actionContext);

def code = "";
for(thing in things){
    //只选择Module
    if(thing.getThingName() != "Module"){
        continue;
    }
    
    def path = thing.getMetadata().getPath();
    if(code == ""){
        code = path;
    }else{
        code = code + "\n" + path;
    }
}

//设置文本
currentModelContext.modulesInput.setText(code);
shell.dispose();
