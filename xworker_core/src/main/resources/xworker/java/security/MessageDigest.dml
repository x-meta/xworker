#$@text#$@
import xworker.dataObject.DataObject;
import java.security.Security;

def datas = [];
def ctx = [:];
def als = [];
als.add("SHA-1");
for(p in Security.getProviders()){
    for(s in p.getServices()){
        if("MessageDigest" != s.getType()){
            continue;
        }
        def al = s.getAlgorithm();
        //def al = s.getType();
        if(ctx.get(al) == null){
            ctx.put(al, al);
            als.add(al);
        }
    }
}
Collections.sort(als);
for(al in als){
    def data = new DataObject("xworker.java.security.MessageDigest/@algorithm/@DataStore/@dataObjects/@AbstractDataObject");
    data.put("id", al);
    data.put("name", al);
    datas.add(data);
}

return datas;
