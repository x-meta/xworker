#$@text#$@
import org.xmeta.ui.session.SessionManager;

def session = SessionManager.getSession(null);
def locale = session.getLocale();
def language = locale.getLanguage();
//println language;
def value = self.doAction("get" + language + "String", actionContext);
if(value != null && value.trim() != ""){
    return value;
}else{
    return self.doAction("getString", actionContext);
}
