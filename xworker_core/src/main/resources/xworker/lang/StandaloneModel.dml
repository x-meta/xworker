#$@text#$@
//菜单
//运行app
def app = currentThing.doAction("getApp", actionContext);
if(app != null){
    app.doAction("run", actionContext);
}

//运行子节点
for(child in currentThing.getChilds()) {
	  child.doAction("run", actionContext);
}
