#$@text#$@
//创建dml.prj
def prj = new File("./dml.prj");
if(!prj.exists()){
    def pw = prj.newPrintWriter();
    pw.println("projectName=");
    pw.close();
}

def thingManager = world.getThingManagers().get(0).getName();
//创建HellowWorld和SimpleThingEditor
for(thing in self.getChilds()){
    if("actions" == thing.getString("name")){
        continue;
    }
    
    if(world.getThing(thing.getString("name")) == null){
        thing = thing.detach();
        thing.getMetadata().setCoderType("dml_xml");
        thing.saveAs(thingManager, thing.getString("name"));
    }
}
