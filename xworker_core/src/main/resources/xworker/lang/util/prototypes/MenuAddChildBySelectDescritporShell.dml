import org.xmeta.Thing;

//获取选中的描述者
def descriptor = descriptorSelector.doAction("getSelected", actionContext);

//添加子节点
def newThing = new Thing(descriptor.getMetadata().getPath());
currentThing.addChild(newThing);
//保存
currentThing.save();

//刷新
editorActions.doAction("refreshOutline", actionContext);

shell.dispose();
