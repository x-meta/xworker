<?xml version="1.0" encoding="utf-8"?>

<Stage name="SliderExample" descriptors="xworker.javafx.stage.Stage">
    <Nodes>
        <VBox name="rootNode" _xmeta_id_="VBox">
            <Slider name="Slider"></Slider>
            <Slider name="Slider" _xmeta_id_="Slider1" showTickLabels="true"></Slider>
            <Slider name="Slider" _xmeta_id_="Slider2" majorTickUnit="10" minorTickCount="5"
                 showTickLabels="true" showTickMarks="true"></Slider>
            <Slider name="Slider" _xmeta_id_="Slider3" majorTickUnit="4" max="100" minorTickCount="2"
                 min="0" showTickLabels="true" showTickMarks="true" snapToTicks="true"
                 valueChanging="true"></Slider>
        </VBox>
    </Nodes>
    <Scene name="Scene" root="var:rootNode"></Scene>
</Stage>