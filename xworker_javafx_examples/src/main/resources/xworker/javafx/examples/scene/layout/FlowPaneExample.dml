<?xml version="1.0" encoding="utf-8"?>

<Stage name="FlowPaneExample" descriptors="xworker.javafx.stage.Stage" title="FlowPaneExample"
     width="200" height="100">
    <description><![CDATA[<p>A JavaFX FlowPane is a layout component which lays out its child components either vertically or horizontally, and which can wrap the components onto the next row or column if there is not enough space in one row.&nbsp;</p>
]]></description>
    <Nodes>
        <FlowPane name="rootNode" _xmeta_id_="FlowPane" hgap="5" vgap="5">
            <Button name="Button" text="Button Example 1"></Button>
            <Button name="Button" _xmeta_id_="Button1" text="Button Example 2"></Button>
            <Button name="Button" _xmeta_id_="Button2" text="Button Example 3"></Button>
        </FlowPane>
    </Nodes>
    <Scene name="Scene" root="var:rootNode"></Scene>
</Stage>