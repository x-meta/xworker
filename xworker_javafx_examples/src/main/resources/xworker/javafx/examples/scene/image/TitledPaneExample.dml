<?xml version="1.0" encoding="utf-8"?>

<Stage name="TitledPaneExample" descriptors="xworker.javafx.stage.Stage">
    <Nodes>
        <VBox name="rootNode" _xmeta_id_="VBox">
            <Resources descriptors="xworker.javafx.stage.Stage/@Resources">
                <Image name="image" url="https://docs.openjfx.cn/images/XR3Player.png"
                     backgroundLoading="true"></Image>
            </Resources>
            <TitledPane title="ImageView1" animated="true">
                <ImageView name="ImageView1" _xmeta_id_="ImageView" fitHeight="300" image="var:image"
                     preserveRatio="true">
                    <description><![CDATA[<p>simple displays ImageView the image as is</p>
]]></description>
                </ImageView>
            </TitledPane>
            <TitledPane _xmeta_id_="TitledPane1" title="ImageView2" animated="true">
                <ImageView name="ImageView1" _xmeta_id_="ImageView" fitHeight="300" image="var:image"
                     preserveRatio="true">
                    <description><![CDATA[<p>simple displays ImageView the image as is</p>
]]></description>
                </ImageView>
            </TitledPane>
        </VBox>
    </Nodes>
    <Scene name="Scene" root="var:rootNode"></Scene>
</Stage>