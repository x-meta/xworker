<?xml version="1.0" encoding="utf-8"?>

<Stage name="ShapesExample" descriptors="xworker.javafx.stage.Stage">
    <Nodes>
        <Pane name="rootNode" _xmeta_id_="Pane">
            <Circle centerX="100" centerY="100" radius="125" strokeWidth="5">
                <Fill>
                    <Color name="Color" color="TRANSPARENT"></Color>
                </Fill>
                <Stroke>
                    <Color name="Color" webColor="#ff00ff"></Color>
                </Stroke>
            </Circle>
            <Rectangle height="400" width="300" x="200" y="200">
                <Fill>
                    <Color name="Color" webColor="#00ffff"></Color>
                </Fill>
                <Stroke>
                    <Color name="Color" color="SIENNA"></Color>
                </Stroke>
            </Rectangle>
        </Pane>
    </Nodes>
    <Scene name="Scene" root="var:rootNode"></Scene>
</Stage>