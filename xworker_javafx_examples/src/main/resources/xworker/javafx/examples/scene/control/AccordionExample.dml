<?xml version="1.0" encoding="utf-8"?>

<Stage name="AccordionExample" descriptors="xworker.javafx.stage.Stage">
    <Nodes>
        <VBox name="rootNode" _xmeta_id_="VBox">
            <Accordion>
                <TitledPane title="Boats">
                    <Label name="Label" text="Show all boats available"></Label>
                </TitledPane>
                <TitledPane _xmeta_id_="TitledPane1" title="Cars">
                    <Label name="Label" text="Show all cars available"></Label>
                </TitledPane>
                <TitledPane _xmeta_id_="TitledPane2" title="Planes">
                    <Label name="Label" text="Show all planes available"></Label>
                </TitledPane>
            </Accordion>
        </VBox>
    </Nodes>
    <Scene name="Scene" root="var:rootNode"></Scene>
</Stage>