StemplateCode
#$@text#$@
angular.module('FileManagerApp').config(['fileManagerConfigProvider', function (config) {
    var defaults = config.$get();
    config.set({
        appName: '${data.metadata.label}',
        defaultLang:'${data.defaultLang?default("en")}',

        listUrl: '${data.serverUrl?if_exists}',
        uploadUrl: '${data.serverUrl?if_exists}',
        renameUrl: '${data.serverUrl?if_exists}',
        copyUrl: '${data.serverUrl?if_exists}',
        moveUrl: '${data.serverUrl?if_exists}',
        removeUrl: '${data.serverUrl?if_exists}',
        editUrl: '${data.serverUrl?if_exists}',
        getContentUrl: '${data.serverUrl?if_exists}',
        createFolderUrl: '${data.serverUrl?if_exists}',
        downloadFileUrl: '${data.serverUrl?if_exists}',
        downloadMultipleUrl: '${data.serverUrl?if_exists}',
        compressUrl: '${data.serverUrl?if_exists}',
        extractUrl: '${data.serverUrl?if_exists}',
        permissionsUrl: '${data.serverUrl?if_exists}',

        ${data.configs}
        
        pickCallback: function(item) {
          var msg = 'Picked %s "%s" for external use'
            .replace('%s', item.type)
            .replace('%s', item.fullPath());
          window.alert(msg);
        },

        allowedActions: angular.extend(defaults.allowedActions, {
          pickFiles: false,
          pickFolders: false,
        }),
   });
 }]);
#$@text#$@
