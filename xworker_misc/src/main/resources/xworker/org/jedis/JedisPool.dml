#$@text#$@
import org.eclipse.swt.SWT;

import org.eclipse.swt.widgets.MessageBox;

import org.xmeta.ActionContext;

def coolBar = actionContext.get("coolBar");
def thing = actionContext.get("currentThing");
def display = coolBar.getShell().display;
def success = success;
def failure = failure;
new Thread({
    display.asyncExec({
        try{
            def ac = new ActionContext();
            def pool = thing.doAction("getJedisPool", actionContext);
            if(pool == null || pool.isClosed()){
                failure(actionContext);
                return;
            }
            success(actionContext);
        }catch(Exception e){
            failure(actionContext);
        }
    });
}).start();
