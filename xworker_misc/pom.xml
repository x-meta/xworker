							<artifactId>tkui-css-swt</artifactId>
							<version>1.0.0</version>
							<packaging>jar</packaging>
							<generatePom>true</generatePom>
						</configuration>
						<goals>
							<goal>install-file</goal>
						</goals>
					</execution>	
				</executions>
			</plugin>
		</plugins>
