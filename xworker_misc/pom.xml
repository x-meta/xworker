							<generatePom>true</generatePom>
						</configuration>
						<goals>
							<goal>install-file</goal>
						</goals>
					</execution>
					<execution>
						<id>install-external4</id>
						<phase>clean</phase>
						<configuration>
							<file>${basedir}/lib/org.akrogen.tkui.css.core-1.0.0_080822.jar</file>
