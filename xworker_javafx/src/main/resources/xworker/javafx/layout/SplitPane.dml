<?xml version="1.0" encoding="utf-8"?>

<thing name="SplitPane" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.control.Control"
     defaultEditor="form">
    <description><![CDATA[<p>A control that has two or more sides, each separated by a divider, which can be dragged by the user to give more space to one of the sides, resulting in the other side shrinking by an equal amount.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.control.SplitPaneActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="orientation" inputtype="select">
        <description><![CDATA[<p>The orientation for the SplitPane.</p>
]]></description>
        <value name="HORIZONTAL" value="HORIZONTAL"></value>
        <value name="VERTICAL" value="VERTICAL"></value>
    </attribute>
    <attribute name="dividerPositions" size="40"></attribute>
    <attribute name="description" inputtype="html"></attribute>
    <VariablesDescs descriptors="xworker.lang.MetaThing/@VariablesDesc">
        <Variable name="var" varName="_thingName_" className="javafx.scene.control.SplitPane"></Variable>
    </VariablesDescs>
</thing>