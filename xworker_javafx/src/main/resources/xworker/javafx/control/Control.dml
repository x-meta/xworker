<?xml version="1.0" encoding="utf-8"?>

<thing name="Control" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.layout.Region">
    <description><![CDATA[<p>Base class for all user interface controls. A &quot;Control&quot; is a node in the scene graph which can be manipulated by the user. Controls provide additional variables and behaviors beyond those of Node to support common user interactions in a manner which is consistent and predictable for the user.</p>
]]></description>
    <thing name="ContextMenu" extends="xworker.javafx.control.ContextMenu" modifier="private"
         inheritDescription="true"></thing>
    <thing name="Tooltip" extends="xworker.javafx.control.Tooltip" inheritDescription="true"></thing>
</thing>