<?xml version="1.0" encoding="utf-8"?>

<thing name="TabPane" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.control.Control"
     icon="/com/oracle/javafx/scenebuilder/kit/editor/images/nodeicons/TabPane.png">
    <description><![CDATA[<p>A control that allows switching between a group of&nbsp;Tabs. Only one tab is visible at a time. Tabs are added to the TabPane by using the&nbsp;getTabs().</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.control.TabPaneActions" methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="rotateGraphic" inputtype="truefalseselect">
        <description><![CDATA[<p>The rotatedGraphic state of the tabs in the TabPane.</p>
]]></description>
    </attribute>
    <attribute name="selectionModel">
        <description><![CDATA[<p>The selection model used for selecting tabs.</p>
]]></description>
    </attribute>
    <attribute name="side" inputtype="select">
        <description><![CDATA[<p>The position of the tabs in the TabPane.</p>
]]></description>
        <value name="BOTTOM" value="BOTTOM"></value>
        <value name="LEFT" value="LEFT"></value>
        <value name="RIGHT" value="RIGHT"></value>
        <value name="TOP" value="TOP"></value>
    </attribute>
    <attribute name="tabClosingPolicy" inputtype="select">
        <description><![CDATA[<p>The closing policy for the tabs.</p>
]]></description>
        <value name="ALL_TABS" value="ALL_TABS"></value>
        <value name="SELECTED_TAB" value="SELECTED_TAB"></value>
        <value name="UNAVAILABLE" value="UNAVAILABLE"></value>
    </attribute>
    <attribute name="tabMaxHeight">
        <description><![CDATA[<p>The maximum height of the tabs in the TabPane.</p>
]]></description>
    </attribute>
    <attribute name="tabMaxWidth">
        <description><![CDATA[<p>The maximum width of the tabs in the TabPane.</p>
]]></description>
    </attribute>
    <attribute name="tabMinHeight">
        <description><![CDATA[<p>The minimum height of the tab.</p>
]]></description>
    </attribute>
    <attribute name="tabMinWidth">
        <description><![CDATA[<p>The minimum width of the tabs in the TabPane.</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
    <VariablesDescs descriptors="xworker.lang.MetaThing/@VariablesDesc">
        <Variable name="var" varName="_thingName_" className="javafx.scene.control.TabPane"></Variable>
    </VariablesDescs>
    <thing name="Tab" extends="xworker.javafx.control.Tab" inheritDescription="true"></thing>
</thing>