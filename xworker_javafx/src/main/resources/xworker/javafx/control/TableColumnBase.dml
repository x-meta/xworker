<?xml version="1.0" encoding="utf-8"?>

<thing name="TableColumnBase" descriptors="xworker.lang.MetaDescriptor3">
    <description><![CDATA[<p>Table-like controls (such as&nbsp;TableView&nbsp;and&nbsp;TreeTableView) are made up of zero or more instances of a concrete TableColumnBase subclass (TableColumn&nbsp;and&nbsp;TreeTableColumn, respectively). Each table column in a table is responsible for displaying (and editing) the contents of that column. As well as being responsible for displaying and editing data for a single column, a table column also contains the necessary properties to:</p>

<ul>
	<li>Be resized (using&nbsp;minWidth/prefWidth/maxWidth&nbsp;and&nbsp;width&nbsp;properties)</li>
	<li>Have its&nbsp;visibility&nbsp;toggled</li>
	<li>Display&nbsp;header text</li>
	<li>Display any&nbsp;nested columns&nbsp;it may contain</li>
	<li>Have a&nbsp;context menu&nbsp;when the user right-clicks the column header area</li>
	<li>Have the contents of the table be sorted (using&nbsp;comparator,&nbsp;sortable&nbsp;and sortType).</li>
</ul>
]]></description>
    <actions>
        <GetObject name="getContextMenu" _xmeta_id_="15598" descriptors="xworker.lang.actions.ActionUtil/@GetData"
             attributeName="contextMenu" usage="params"></GetObject>
        <GetObject name="getGraphic" _xmeta_id_="15599" descriptors="xworker.lang.actions.ActionUtil/@GetData"
             attributeName="graphic" usage="params"></GetObject>
        <GetString name="getText" _xmeta_id_="15600" descriptors="xworker.lang.actions.ActionUtil/@GetString"
             attributeName="text" usage="params"></GetString>
    </actions>
    <attribute name="comparator" group="TableColumnBase">
        <description><![CDATA[<p>Comparator function used when sorting this table column.</p>
]]></description>
    </attribute>
    <attribute name="contextMenu" inputtype="openWindow" size="40" group="TableColumnBase"
         inputattrs="xworker.swt.xworker.attributeEditor.openWins.SelectThingOpenWindow/@shell|prefix=var%3A,descriptor=xworker.javafx.control.ContextMenu,seperator=%2C,returnType=name,append=false">
        <description><![CDATA[<p>This menu will be shown whenever the user right clicks within the header area of this TableColumnBase.</p>
]]></description>
    </attribute>
    <attribute name="editable" inputtype="truefalseselect" group="TableColumnBase">
        <description><![CDATA[<p>Specifies whether this table column allows editing.</p>
]]></description>
    </attribute>
    <attribute name="graphic" inputtype="openWindow" size="60" group="TableColumnBase" inputattrs="xworker.swt.xworker.attributeEditor.openWins.SelectThingOpenWindow/@shell|prefix=var%3A,descriptor=xworker.javafx.scene.Node,seperator=%2C,returnType=name,append=false">
        <description><![CDATA[<p>The graphic to show in the table column to allow the user to indicate graphically what is in the column.</p>
]]></description>
    </attribute>
    <attribute name="id" group="TableColumnBase">
        <description><![CDATA[<p>The id of this TableColumnBase.</p>
]]></description>
    </attribute>
    <attribute name="maxWidth" group="TableColumnBase">
        <description><![CDATA[<p>The maximum width the table column is permitted to be resized to.</p>
]]></description>
    </attribute>
    <attribute name="minWidth" group="TableColumnBase">
        <description><![CDATA[<p>The minimum width the table column is permitted to be resized to.</p>
]]></description>
    </attribute>
    <attribute name="prefWidth" group="TableColumnBase">
        <description><![CDATA[<p>The preferred width of the TableColumn.</p>
]]></description>
    </attribute>
    <attribute name="resizable" inputtype="truefalseselect" group="TableColumnBase">
        <description><![CDATA[<p>Used to indicate whether the width of this column can change.</p>
]]></description>
    </attribute>
    <attribute name="sortable" inputtype="truefalseselect" group="TableColumnBase">
        <description><![CDATA[<p>A boolean property to toggle on and off the 'sortability' of this column.</p>
]]></description>
    </attribute>
    <attribute name="sortNode" group="TableColumnBase">
        <description><![CDATA[<p>The node to use as the "sort arrow", shown to the user in situations where the table column is part of the sort order.</p>
]]></description>
    </attribute>
    <attribute name="text" size="50" group="TableColumnBase">
        <description><![CDATA[<p>This is the text to show in the header for this column.</p>
]]></description>
    </attribute>
    <attribute name="visible" inputtype="truefalseselect" group="TableColumnBase">
        <description><![CDATA[<p>Toggling this will immediately toggle the visibility of this column, and all children columns.</p>
]]></description>
    </attribute>
    <attribute name="style" inputtype="textarea" group="TableColumnBase" inputattrs="codeName=css codeType=css wrap=false fillBoth=true cols= rows= textAssistor=">
        <description><![CDATA[<p>A string representation of the CSS style associated with this TableColumnBase instance.</p>
]]></description>
    </attribute>
    <thing name="ContextMenu" extends="xworker.javafx.control.ContextMenu" inheritDescription="true"></thing>
    <thing name="Graphic" extends="xworker.javafx.control.Graphic" inheritDescription="true"></thing>
</thing>