<?xml version="1.0" encoding="utf-8"?>

<thing name="MenuItem" descriptors="xworker.lang.MetaDescriptor3">
    <description><![CDATA[<p>MenuItem is intended to be used in conjunction with&nbsp;Menu&nbsp;to provide options to users. MenuItem serves as the base class for the bulk of JavaFX menus API. It has a display&nbsp;text&nbsp;property, as well as an optional&nbsp;graphic&nbsp;node that can be set on it. The&nbsp;accelerator&nbsp;property enables accessing the associated action in one keystroke. Also, as with the&nbsp;Button&nbsp;control, by using the&nbsp;setOnAction(javafx.event.EventHandler&lt;javafx.event.ActionEvent&gt;)&nbsp;method, you can have an instance of MenuItem perform any action you wish.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.control.MenuItemActions"
             methodName="create"></JavaAction>
        <GetObject name="getGraphic" descriptors="xworker.lang.actions.ActionUtil/@GetData"
             attributeName="graphic"></GetObject>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="accelerator" group="MenuItem">
        <description><![CDATA[<p>The accelerator property enables accessing the associated action in one keystroke.</p>
]]></description>
    </attribute>
    <attribute name="disable" inputtype="truefalseselect" group="MenuItem">
        <description><![CDATA[<p>Sets the individual disabled state of this MenuItem.</p>
]]></description>
    </attribute>
    <attribute name="graphic" inputtype="openWindow" size="50" group="MenuItem" inputattrs="xworker.swt.xworker.attributeEditor.openWins.SelectThingOpenWindow/@shell|prefix=var%3A,descriptor=xworker.javafx.scene.Node,seperator=%2C,returnType=name,append=false">
        <description><![CDATA[<p>An optional graphic for the MenuItem.</p>
]]></description>
    </attribute>
    <attribute name="id" group="MenuItem">
        <description><![CDATA[<p>The id of this MenuItem.</p>
]]></description>
    </attribute>
    <attribute name="mnemonicParsing" group="MenuItem">
        <description><![CDATA[<p>MnemonicParsing property to enable/disable text parsing.</p>
]]></description>
    </attribute>
    <attribute name="style" group="MenuItem">
        <description><![CDATA[<p>A string representation of the CSS style associated with this specific MenuItem.</p>
]]></description>
    </attribute>
    <attribute name="text" size="50">
        <description><![CDATA[<p>The text to display in the MenuItem.</p>
]]></description>
    </attribute>
    <attribute name="visible" inputtype="truefalseselect" group="MenuItem">
        <description><![CDATA[<p>Specifies whether this MenuItem should be rendered as part of the scene graph.</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
    <thing name="MenuItemEventHandler" _xmeta_id_="EventHandler" descriptors="xworker.lang.MetaDescriptor3"
         extends="xworker.lang.actions.Actions">
        <actions>
            <JavaAction name="create" outerClassName="xworker.javafx.event.ThingEventHandler"
                 methodName="create"></JavaAction>
        </actions>
        <attribute name="name" _xmeta_id_="name1"></attribute>
        <attribute name="label"></attribute>
        <attribute name="type" _xmeta_id_="name" inputtype="select">
            <value name="onAction" value="onAction"></value>
            <value name="onMenuValidation" value="onMenuValidation"></value>
        </attribute>
        <attribute name="ref" inputtype="dataSelector" size="60">
            <description><![CDATA[<p><strong>引用规则：</strong></p>

<ul>
	<li><strong>xxx:aaa</strong><br />
	执行名字为xxx的ActionContainer的aaa方法。</li>
	<li><strong>默认引用其它EventHandler</strong>，即执行引用模型的子节点（作为动作）。</li>
</ul>
]]></description>
        </attribute>
        <attribute name="description" inputtype="html"></attribute>
        <VariablesDescs descriptors="xworker.lang.MetaThing/@VariablesDesc">
            <Variable name="event" _xmeta_id_="var" varName="event" className="javafx.event.Event"></Variable>
        </VariablesDescs>
    </thing>
    <VariablesDescs descriptors="xworker.lang.MetaThing/@VariablesDesc">
        <Variable name="var" varName="_thingName_" className="javafx.scene.control.MenuItem"></Variable>
    </VariablesDescs>
    <thing name="Graphic" extends="xworker.javafx.control.Graphic"></thing>
</thing>