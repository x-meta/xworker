<?xml version="1.0" encoding="utf-8"?>

<thing name="ScrollPane" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.control.Control,xworker.javafx.scene.Nodes"
     icon="/com/oracle/javafx/scenebuilder/kit/editor/images/nodeicons/ScrollPane.png">
    <description><![CDATA[<p>A Control that provides a scrolled, clipped viewport of its contents. It allows the user to scroll the content around either directly (panning) or by using scroll bars. The ScrollPane allows specification of the scroll bar policy, which determines when scroll bars are displayed: always, never, or only when they are needed. The scroll bar policy can be specified independently for the horizontal and vertical scroll bars.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.control.ScrollPaneActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="content">
        <description><![CDATA[<p>The node used as the content of this ScrollPane.</p>
]]></description>
    </attribute>
    <attribute name="fitToHeight" inputtype="truefalseselect">
        <description><![CDATA[<p>If true and if the contained node is a Resizable, then the node will be kept resized to match the height of the ScrollPane's viewport.</p>
]]></description>
    </attribute>
    <attribute name="fitToWidth" inputtype="truefalseselect">
        <description><![CDATA[<p>If true and if the contained node is a Resizable, then the node will be kept resized to match the width of the ScrollPane's viewport.</p>
]]></description>
    </attribute>
    <attribute name="hbarPolicy" inputtype="select">
        <description><![CDATA[<p>Specifies the policy for showing the horizontal scroll bar.</p>
]]></description>
        <value name="ALWAYS" value="ALWAYS"></value>
        <value name="AS_NEEDED" value="AS_NEEDED"></value>
        <value name="NEVER" value="NEVER"></value>
    </attribute>
    <attribute name="hmax">
        <description><![CDATA[<p>The maximum allowable hvalue for this ScrollPane.</p>
]]></description>
    </attribute>
    <attribute name="hmin">
        <description><![CDATA[<p>The minimum allowable hvalue for this ScrollPane.</p>
]]></description>
    </attribute>
    <attribute name="hvalue">
        <description><![CDATA[<p>The current horizontal scroll position of the ScrollPane.</p>
]]></description>
    </attribute>
    <attribute name="minViewportHeight">
        <description><![CDATA[<p>Specify the minimum height of the ScrollPane Viewport.</p>
]]></description>
    </attribute>
    <attribute name="minViewportWidth">
        <description><![CDATA[<p>Specify the minimum width of the ScrollPane Viewport.</p>
]]></description>
    </attribute>
    <attribute name="pannable" inputtype="truefalseselect">
        <description><![CDATA[<p>Specifies whether the user should be able to pan the viewport by using the mouse.</p>
]]></description>
    </attribute>
    <attribute name="prefViewportHeight">
        <description><![CDATA[<p>Specify the preferred height of the ScrollPane Viewport.</p>
]]></description>
    </attribute>
    <attribute name="prefViewportWidth">
        <description><![CDATA[<p>Specify the perferred width of the ScrollPane Viewport.</p>
]]></description>
    </attribute>
    <attribute name="vbarPolicy" extends="xworker.javafx.control.ScrollPane/@hbarPolicy"
         inputtype="truefalseselect">
        <description><![CDATA[<p>Specifies the policy for showing the vertical scroll bar.</p>
]]></description>
    </attribute>
    <attribute name="viewportBounds">
        <description><![CDATA[<p>The actual Bounds of the ScrollPane Viewport.</p>
]]></description>
    </attribute>
    <attribute name="vmax">
        <description><![CDATA[<p>The maximum allowable vvalue for this ScrollPane.</p>
]]></description>
    </attribute>
    <attribute name="vmin">
        <description><![CDATA[<p>The minimum allowable vvalue for this ScrollPane.</p>
]]></description>
    </attribute>
    <attribute name="vvalue">
        <description><![CDATA[<p>The current vertical scroll position of the ScrollPane.</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
    <VariablesDescs descriptors="xworker.lang.MetaThing/@VariablesDesc">
        <Variable name="var" varName="_thingName_" className="javafx.scene.control.ScrollPane"></Variable>
    </VariablesDescs>
</thing>