<?xml version="1.0" encoding="utf-8"?>

<thing name="Image" descriptors="xworker.lang.MetaDescriptor3">
    <description><![CDATA[<p>The&nbsp;Image&nbsp;class represents graphical images and is used for loading images from a specified URL.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.scene.image.ImageActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="url" inputtype="file" size="60">
        <description><![CDATA[<p>可以是InputStream或String表示的url。</p>
]]></description>
    </attribute>
    <attribute name="requestedWidth">
        <description><![CDATA[<p>the image&#39;s bounding box width</p>
]]></description>
    </attribute>
    <attribute name="requestedHeight">
        <description><![CDATA[<p>the image&#39;s bounding box height</p>
]]></description>
    </attribute>
    <attribute name="preserveRatio" inputtype="truefalseselect">
        <description><![CDATA[<p>indicates whether to preserve the aspect ratio of the original image when scaling to fit the image within the specified bounding box</p>
]]></description>
    </attribute>
    <attribute name="smooth" inputtype="truefalseselect">
        <description><![CDATA[<p>indicates whether to use a better quality filtering algorithm or a faster one when scaling this image to fit within the specified bounding box</p>
]]></description>
    </attribute>
    <attribute name="backgroundLoading" inputtype="truefalseselect">
        <description><![CDATA[<p>indicates whether the image is being loaded in the background</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
    <VariablesDescs descriptors="xworker.lang.MetaThing/@VariablesDesc">
        <Variable name="var" varName="_thingName_" className="javafx.scene.image.Image"></Variable>
    </VariablesDescs>
</thing>