<?xml version="1.0" encoding="utf-8"?>

<thing name="WebView" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.Parent,xworker.javafx.scene.web.WebEngine"
     icon="/com/oracle/javafx/scenebuilder/kit/editor/images/nodeicons/WebView.png">
    <description><![CDATA[<p>WebView&nbsp;is a&nbsp;Node&nbsp;that manages a&nbsp;WebEngine&nbsp;and displays its content. The associated&nbsp;WebEngine&nbsp;is created automatically at construction time and cannot be changed afterwards.&nbsp;WebView&nbsp;handles mouse and some keyboard events, and manages scrolling automatically, so there&#39;s no need to put it into a&nbsp;ScrollPane.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.scene.web.WebViewActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="contextMenuEnabled" inputtype="truefalseselect">
        <description><![CDATA[<p>Specifies whether context menu is enabled.</p>
]]></description>
    </attribute>
    <attribute name="fontScale">
        <description><![CDATA[<p>Specifies scale factor applied to font.</p>
]]></description>
    </attribute>
    <attribute name="fontSmoothingType" extends="xworker.javafx.scene.text.FontSmoothingType/@Value"
         inputtype="select">
        <description><![CDATA[<p>Specifies a requested font smoothing type : gray or LCD.</p>
]]></description>
    </attribute>
    <attribute name="maxHeight">
        <description><![CDATA[<p>Maximum height property.</p>
]]></description>
    </attribute>
    <attribute name="maxWidth">
        <description><![CDATA[<p>Maximum width property.</p>
]]></description>
    </attribute>
    <attribute name="minHeight">
        <description><![CDATA[<p>Minimum height property.</p>
]]></description>
    </attribute>
    <attribute name="minWidth">
        <description><![CDATA[<p>Minimum width property.</p>
]]></description>
    </attribute>
    <attribute name="prefHeight">
        <description><![CDATA[<p>Preferred height property.</p>
]]></description>
    </attribute>
    <attribute name="prefWidth">
        <description><![CDATA[<p>Preferred width property.</p>
]]></description>
    </attribute>
    <attribute name="zoom">
        <description><![CDATA[<p>Returns zoom property object.</p>
]]></description>
    </attribute>
    <attribute name="url" size="60"></attribute>
    <attribute name="content" inputtype="textarea"></attribute>
    <attribute name="description" inputtype="html"></attribute>
    <VariablesDescs descriptors="xworker.lang.MetaThing/@VariablesDesc">
        <Variable name="var" varName="_thingName_" className="javafx.scene.web.WebView"></Variable>
    </VariablesDescs>
</thing>