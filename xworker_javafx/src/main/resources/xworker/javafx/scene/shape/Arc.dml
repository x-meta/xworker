<?xml version="1.0" encoding="utf-8"?>

<thing name="Arc" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.shape.Shape"
     icon="/com/oracle/javafx/scenebuilder/kit/editor/images/nodeicons/Arc.png">
    <description><![CDATA[<p>The&nbsp;Arc&nbsp;class represents a 2D arc object, defined by a center point, start angle (in degrees), angular extent (length of the arc in degrees), and an arc type (ArcType.OPEN,&nbsp;ArcType.CHORD, or&nbsp;ArcType.ROUND).</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.scene.shape.ArcActions" methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="centerX">
        <description><![CDATA[<p>Defines the X coordinate of the center point of the arc.</p>
]]></description>
    </attribute>
    <attribute name="centerY">
        <description><![CDATA[<p>Defines the Y coordinate of the center point of the arc.</p>
]]></description>
    </attribute>
    <attribute name="length">
        <description><![CDATA[<p>Defines the angular extent of the arc in degrees.</p>
]]></description>
    </attribute>
    <attribute name="radiusX">
        <description><![CDATA[<p>Defines the overall width (horizontal radius) of the full ellipse of which this arc is a partial section.</p>
]]></description>
    </attribute>
    <attribute name="radiusY">
        <description><![CDATA[<p>Defines the overall height (vertical radius) of the full ellipse of which this arc is a partial section.</p>
]]></description>
    </attribute>
    <attribute name="startAngle">
        <description><![CDATA[<p>Defines the starting angle of the arc in degrees.</p>
]]></description>
    </attribute>
    <attribute name="type" inputtype="select">
        <description><![CDATA[<p>Defines the closure type for the arc: ArcType.OPEN, ArcType.CHORD,or ArcType.ROUND.</p>
]]></description>
        <value name="CHORD" value="CHORD"></value>
        <value name="OPEN" value="OPEN"></value>
        <value name="ROUND" value="ROUND"></value>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
    <VariablesDescs descriptors="xworker.lang.MetaThing/@VariablesDesc">
        <Variable name="var" varName="_thingName_" className="javafx.scene.shape.Arc"></Variable>
    </VariablesDescs>
</thing>