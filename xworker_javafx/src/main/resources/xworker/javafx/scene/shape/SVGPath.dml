<thing name="SVGPath" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.shape.Shape">
    <description><![CDATA[<p>The&nbsp;SVGPath&nbsp;class represents a simple shape that is constructed by parsing SVG path data from a String.</p>

<pre class="brush:java;">
import javafx.scene.shape.*;

SVGPath svg = new SVGPath();
svg.setContent(&quot;M40,60 C42,48 44,30 25,32&quot;);</pre>

<p>&nbsp;</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.scene.shape.SVGPathActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="content" inputtype="textarea"></attribute>
    <attribute name="fillRule" inputtype="select">
        <value name="EVEN_ODD" value="EVEN_ODD"></value>
        <value name="NON_ZERO" value="NON_ZERO"></value>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
</thing>