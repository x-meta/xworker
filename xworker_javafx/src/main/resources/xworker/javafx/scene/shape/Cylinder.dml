<?xml version="1.0" encoding="utf-8"?>

<thing name="Cylinder" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.shape.Shape3D">
    <description><![CDATA[<p>The&nbsp;Cylinder&nbsp;class defines a 3 dimensional cylinder with the specified size. A&nbsp;Cylinder&nbsp;is a 3D geometry primitive created with a given radius and height. It is centered at the origin.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.scene.shape.CylinderActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="height">
        <description><![CDATA[<p>Defines the height or the Y dimension of the Cylinder.</p>
]]></description>
    </attribute>
    <attribute name="radius">
        <description><![CDATA[<p>Defines the radius in the Z plane of the Cylinder.</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
</thing>