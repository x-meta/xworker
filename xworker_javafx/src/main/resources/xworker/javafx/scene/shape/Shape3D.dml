<?xml version="1.0" encoding="utf-8"?>

<thing name="Shape3D" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.Node">
    <description><![CDATA[<p>The&nbsp;Shape3D&nbsp;base class provides definitions of common properties for objects that represent some form of 3D geometric shape.&nbsp;</p>
]]></description>
    <attribute name="cullFace" inputtype="select" group="Shape3D">
        <description><![CDATA[<p>Defines the cullFace this Shape3D.</p>
]]></description>
        <value name="BACK" value="BACK"></value>
        <value name="FRONT" value="FRONT"></value>
        <value name="NONE" value="NONE"></value>
    </attribute>
    <attribute name="drawMode" inputtype="select" group="Shape3D">
        <description><![CDATA[<p>Defines the draw mode used to render this Shape3D.</p>
]]></description>
        <value name="FILL" value="FILL"></value>
        <value name="LINE" value="LINE"></value>
    </attribute>
    <attribute name="material" group="Shape3D">
        <description><![CDATA[<p>Defines the material this Shape3D.</p>
]]></description>
    </attribute>
</thing>