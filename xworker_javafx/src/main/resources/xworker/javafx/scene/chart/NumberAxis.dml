<?xml version="1.0" encoding="utf-8"?>

<thing name="NumberAxis" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.chart.ValueAxis"
     icon="/com/oracle/javafx/scenebuilder/kit/editor/images/nodeicons/NumberAxis.png">
    <description><![CDATA[<p>A axis class that plots a range of numbers with major tick marks every &quot;tickUnit&quot;. You can use any Number type with this axis, Long, Double, BigDecimal etc.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.scene.chart.NumberAxisActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label" size="50"></attribute>
    <attribute name="forceZeroInRange" inputtype="truefalseselect">
        <description><![CDATA[<p>When true zero is always included in the visible range.</p>
]]></description>
    </attribute>
    <attribute name="tickUnit">
        <description><![CDATA[<p>The tick unit, ie space between tickmarks</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
</thing>