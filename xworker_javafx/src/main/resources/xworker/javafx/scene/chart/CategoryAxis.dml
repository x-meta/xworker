<?xml version="1.0" encoding="utf-8"?>

<thing name="CategoryAxis" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.chart.Axis"
     icon="/com/oracle/javafx/scenebuilder/kit/editor/images/nodeicons/CategoryAxis.png">
    <description><![CDATA[<p>A axis implementation that will works on string categories where each value as a unique category(tick mark) along the axis.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.scene.chart.CategoryAxisActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="endMargin">
        <description><![CDATA[<p>The margin between the last tick mark and the axis end</p>
]]></description>
    </attribute>
    <attribute name="gapStartAndEnd" inputtype="truefalseselect">
        <description><![CDATA[<p>If this is true then half the space between ticks is left at the start and end</p>
]]></description>
    </attribute>
    <attribute name="startMargin">
        <description><![CDATA[<p>The margin between the axis start and the first tick-mark</p>
]]></description>
    </attribute>
    <attribute name="categories" size="60" addonLabelColor="COLOR_BLUE">
        <description><![CDATA[<p>List of the categories for this axis</p>

<p>接收ObservableList&lt;String&gt;或Iterable&lt;String&gt;。</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
</thing>