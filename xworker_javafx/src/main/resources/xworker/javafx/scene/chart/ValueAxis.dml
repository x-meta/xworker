<?xml version="1.0" encoding="utf-8"?>

<thing name="ValueAxis" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.chart.Axis">
    <description><![CDATA[<p>A axis who&#39;s data is defined as Numbers. It can also draw minor tick-marks between the major ones.</p>
]]></description>
    <attribute name="lowerBound" group="ValueAxis">
        <description><![CDATA[<p>The value for the lower bound of this axis, ie min value.</p>
]]></description>
    </attribute>
    <attribute name="minorTickCount" group="ValueAxis">
        <description><![CDATA[<p>The number of minor tick divisions to be displayed between each major tick mark.</p>
]]></description>
    </attribute>
    <attribute name="minorTickLength" group="ValueAxis">
        <description><![CDATA[<p>The length of minor tick mark lines.</p>
]]></description>
    </attribute>
    <attribute name="minorTickVisible" inputtype="truefalseselect" group="ValueAxis">
        <description><![CDATA[<p>true if minor tick marks should be displayed</p>
]]></description>
    </attribute>
    <attribute name="tickLabelFormatter" size="40" group="ValueAxis">
        <description><![CDATA[<p>StringConverter used to format tick mark labels.</p>
]]></description>
    </attribute>
    <attribute name="upperBound" group="ValueAxis">
        <description><![CDATA[<p>The value for the upper bound of this axis, ie max value.</p>
]]></description>
    </attribute>
    <thing name="TickLabelFormatter" extends="xworker.javafx.util.StringConverters">
        <description><![CDATA[<p>StringConverter used to format tick mark labels.</p>
]]></description>
        <actions>
            <JavaAction name="create" outerClassName="xworker.javafx.scene.chart.ValueAxisActions"
                 methodName="createTickLabelFormatter"></JavaAction>
        </actions>
        <attribute name="name"></attribute>
        <attribute name="label"></attribute>
        <attribute name="description" inputtype="html"></attribute>
    </thing>
</thing>