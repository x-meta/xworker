<?xml version="1.0" encoding="utf-8"?>

<thing name="PieChart" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.chart.Chart">
    <description><![CDATA[<p>Displays a PieChart. The chart content is populated by pie slices based on data set on the PieChart.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.javafx.scene.chart.PieChartActions"
             methodName="create"></JavaAction>
        <GetObject name="getData" _xmeta_id_="19270" descriptors="xworker.lang.actions.ActionUtil/@GetData"
             attributeName="data" usage="params"></GetObject>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="clockwise" inputtype="truefalseselect">
        <description><![CDATA[<p>When true we start placing slices clockwise from the startAngle</p>
]]></description>
    </attribute>
    <attribute name="data" size="40">
        <description><![CDATA[<p>PieCharts data</p>
]]></description>
    </attribute>
    <attribute name="labelLineLength">
        <description><![CDATA[<p>The length of the line from the outside of the pie to the slice labels.</p>
]]></description>
    </attribute>
    <attribute name="labelsVisible" inputtype="truefalseselect">
        <description><![CDATA[<p>When true pie slice labels are drawn</p>
]]></description>
    </attribute>
    <attribute name="startAngle">
        <description><![CDATA[<p>The angle to start the first pie slice at</p>
]]></description>
    </attribute>
    <attribute name="title" size="50">
        <description><![CDATA[<p>The chart title</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
</thing>