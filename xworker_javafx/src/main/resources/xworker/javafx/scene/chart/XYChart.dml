<?xml version="1.0" encoding="utf-8"?>

<thing name="XYChart" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.javafx.scene.chart.Chart">
    <description><![CDATA[<p>Chart base class for all 2 axis charts. It is responsible for drawing the two axes and the plot content. It contains a list of all content in the plot and implementations of XYChart can add nodes to this list that need to be rendered.</p>
]]></description>
    <actions>
        <GetObject name="getData" _xmeta_id_="20169" descriptors="xworker.lang.actions.ActionUtil/@GetData"
             attributeName="data" usage="params"></GetObject>
    </actions>
    <attribute name="alternativeColumnFillVisible" inputtype="truefalseselect" group="XYChart">
        <description><![CDATA[<p>If true then alternative vertical columns will have fills</p>
]]></description>
    </attribute>
    <attribute name="alternativeRowFillVisible" inputtype="truefalseselect" group="XYChart">
        <description><![CDATA[<p>If true then alternative horizontal rows will have fills</p>
]]></description>
    </attribute>
    <attribute name="data" size="40" group="XYChart">
        <description><![CDATA[<p>XYCharts data</p>
]]></description>
    </attribute>
    <attribute name="horizontalGridLinesVisible" inputtype="truefalseselect" group="XYChart">
        <description><![CDATA[<p>True if horizontal grid lines should be drawn</p>
]]></description>
    </attribute>
    <attribute name="horizontalZeroLineVisible" inputtype="truefalseselect" group="XYChart">
        <description><![CDATA[<p>If this is true and the horizontal axis has both positive and negative values then a additional axis line will be drawn at the zero point</p>
]]></description>
    </attribute>
    <attribute name="verticalGridLinesVisible" inputtype="truefalseselect" group="XYChart">
        <description><![CDATA[<p>True if vertical grid lines should be drawn</p>
]]></description>
    </attribute>
    <attribute name="verticalZeroLineVisible" inputtype="truefalseselect" group="XYChart">
        <description><![CDATA[<p>If this is true and the vertical axis has both positive and negative values then a additional axis line will be drawn at the zero point</p>
]]></description>
    </attribute>
    <attribute name="xAxis" group="XYChart">
        <description><![CDATA[<p>X Axis for this XY chart</p>
]]></description>
    </attribute>
    <attribute name="yAxis" group="XYChart">
        <description><![CDATA[<p>Y Axis for this XY chart</p>
]]></description>
    </attribute>
    <thing name="XAxis" extends="xworker.javafx.scene.chart.Axiss">
        <description><![CDATA[<p>X Axis for this XY chart</p>
]]></description>
        <attribute name="name"></attribute>
        <attribute name="label"></attribute>
        <attribute name="description" inputtype="html"></attribute>
    </thing>
    <thing name="YAxis" extends="xworker.javafx.scene.chart.Axiss">
        <description><![CDATA[<p>Y Axis for this XY chart</p>
]]></description>
        <attribute name="name"></attribute>
        <attribute name="label"></attribute>
        <attribute name="description" inputtype="html"></attribute>
    </thing>
</thing>