    Scode=
#$@text#$@
def thing = world.getThing(id);
if(thing == null){
    if("__TreeNodeRootId__" == id){
        def node = self.getThing("Node@0");
        if(node != null){
            thing = node;
        }else{
            return [];
        }
    }else{
        return self.doAction("getRoot", actionContext);
    }
    //thing = self;
}

def nodes = [];
for(child in thing.getChilds()){
    def node = child.doAction("toNode", actionContext);
    if(node != null){
        nodes.add(node);
    }
}

return nodes;
