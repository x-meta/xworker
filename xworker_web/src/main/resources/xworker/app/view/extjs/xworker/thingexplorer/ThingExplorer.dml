sname
click
sparams
node,e
suseChildsCode
false
Scode
#$@text#$@
var path = node.attributes.dataId;
var type = node.attributes.tabId;
var thingManager = null;
var ok = false;
var editButton = Ext.getCmp('explorerEditThingButton');
editButton.setDisabled(true);

if(type == "category"){
    var parentNode = node.parentNode;
    while(parentNode.attributes.tabId != "thingManager"){
       parentNode = parentNode.parentNode;
    }
    
    thingManager = parentNode.attributes.dataId;
    ok = true;
}else if(type == "thingManager"){
    ok = true;
    thingManager = path;
    path = "";    
}

if(ok){
   var store = Ext.getCmp('explorer_thing_grid').store;
   var storeParams = {thingManager:thingManager, category:path};
   Ext.apply(store.baseParams, storeParams );
   store.load({params: storeParams});
}
#$@text#$@
