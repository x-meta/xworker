<?xml version="1.0" encoding="utf-8"?>

<Things name="WebThings" descriptors="xworker.lang.Things">
    <Category name="xworker">
        <Category name="web" _xmeta_id_="Category" descriptors="xworker.ide.config.decriptors.Things/@Category/@Category"
             description="&lt;p&gt;Web应用可以通过浏览器打开。&lt;/p&gt;">
            <Category name="misc">
                <Thing name="ControlSet" _xmeta_id_="simpleDynamicAction" label="WEB集合"
                     thingPath="xworker.http.templates.DynamicActionWebTpl" descriptor="xworker.http.ControlSet"
                     description="&lt;p&gt;最常用的用于创建WEB页面的事物，继承了一些常用的WebControl。&lt;/p&gt;"></Thing>
            </Category>
            <Category name="controls">
                <Thing name="SimpleControl" descriptor="xworker.http.controls.SimpleControl"></Thing>
                <Thing name="WebControlMapping" descriptor="xworker.ide.config.decriptors.WebControlMapping"></Thing>
                <Thing name="ControlSet" descriptor="xworker.http.ControlSet"></Thing>
                <Thing name="REST" descriptor="xworker.http.controls.REST"></Thing>
                <Thing name="JarControl" descriptor="xworker.http.controls.JarControl"></Thing>
            </Category>
            <Category name="view" _xmeta_id_="html">
                <Thing name="view" label="界面" descriptor="xworker.html.base.view"></Thing>
            </Category>
            <Category name="application">
                <Category name="simple">
                    <Thing name="simpleControl" label="简单页面" thingType="template" thingPath="xworker.http.templates.SimpleWebPageTpl"
                         descriptor="xworker.http.ControlSet" description="&lt;p&gt;一个仅包含页面的简单WEB。&lt;/p&gt;"></Thing>
                    <Thing name="simpleDynamicPage" label="动态页面" thingType="template"
                         thingPath="xworker.http.templates.DynamicSimpleWebTpl" descriptor="xworker.http.ControlSet"
                         description="&lt;p&gt;有动作、页面和结果的mvc动态WEB界面。&lt;/p&gt;"></Thing>
                    <Thing name="SimpleFrameApp" descriptor="xworker.app.view.http.app.SimpleFrameApp"></Thing>
                    <Thing name="SimpleLogin" descriptor="xworker.app.view.http.app.SimpleLogin"></Thing>
                    <Category name="extjs">
                        <Thing name="TitleTreeApp" descriptor="xworker.app.view.extjs.widgets.app.TitleTreeApp"></Thing>
                        <Thing name="SimpleApp1" descriptor="xworker.app.view.extjs.application.simple1.SimpleApp1"></Thing>
                    </Category>
                </Category>
                <Category name="XWorkerWeb" label="xworkerWeb" description="&lt;p&gt;XWorker的网站界面。&lt;/p&gt;">
                    <Thing name="Module" descriptor="xworker.doc.structures.Module"></Thing>
                    <Thing name="HtmlContent" descriptor="xworker.doc.structures.HtmlContent"></Thing>
                    <Thing name="HtmlContents" descriptor="xworker.doc.structures.HtmlContents"></Thing>
                </Category>
                <Category name="doc">
                    <Thing name="TreeHtmlDoc" descriptor="xworker.doc.treehtmldoc.TreeHtmlDoc"></Thing>
                    <Thing name="SimpleDoc" descriptor="xworker.doc.structures.SimpleDoc"></Thing>
                    <Thing name="OrgDocument" descriptor="xworker.app.web.docorg.OrgDocument"></Thing>
                </Category>
                <Category name="file">
                    <Thing name="AngularFilemanager" descriptor="xworker.web.angular.AngularFilemanager"></Thing>
                </Category>
            </Category>
            <Category name="javascript" descriptors="xworker.ide.config.decriptors.Things/@Category">
                <Thing name="JavaScriptObjectType" descriptor="xworker.html.base.scripts.JavaScriptObjectType"></Thing>
            </Category>
            <Category name="server">
                <Thing name="SimpleHttpServer" descriptor="xworker.http.server.SimpleHttpServer"></Thing>
                <Thing name="JettyServer" descriptor="xworker.http.server.jetty.JettyServer"></Thing>
            </Category>
            <Category name="extjs" _xmeta_id_="Extjs">
                <Thing name="ExtJs" descriptor="xworker.html.extjs.ExtJs"></Thing>
                <Thing name="RemoteWidgets" descriptor="xworker.html.extjs.xw.remote.RemoteWidgets"></Thing>
                <Thing name="RemoteWidget" descriptor="xworker.html.extjs.xw.remote.RemoteWidget"></Thing>
                <Category name="apps">
                    <Thing name="TitleTreeApp" descriptor="xworker.app.view.extjs.widgets.app.TitleTreeApp"></Thing>
                    <Thing name="SimpleApp1" descriptor="xworker.app.view.extjs.application.simple1.SimpleApp1"></Thing>
                </Category>
                <Category name="grid">
                    <Thing name="DataObjectGridNewDialog" descriptor="xworker.app.view.extjs.widgets.grid.DataObjectGridNewDialog"></Thing>
                </Category>
            </Category>
            <Category name="websocket">
                <Thing name="JettyWebSocket" descriptor="xworker.websocket.jetty.JettyWebSocket"></Thing>
            </Category>
        </Category>
    </Category>
</Things>