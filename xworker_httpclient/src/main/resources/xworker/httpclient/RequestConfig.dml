<?xml version="1.0" encoding="utf-8"?>

<thing name="RequestConfig" descriptors="xworker.lang.MetaDescriptor3">
    <description><![CDATA[<p>Immutable class encapsulating request configuration items. The default setting for stale connection checking changed to false, and the feature was deprecated starting with version 4.4.</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.httpclient.RequestConfigActions"
             methodName="create"></JavaAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="authenticationEnabled" inputtype="truefalseselect"></attribute>
    <attribute name="circularRedirectsAllowed" inputtype="truefalseselect"></attribute>
    <attribute name="connectionRequestTimeout"></attribute>
    <attribute name="connectTimeout"></attribute>
    <attribute name="contentCompressionEnabled" inputtype="truefalseselect"></attribute>
    <attribute name="cookieSpec" size="50"></attribute>
    <attribute name="expectContinueEnabled" inputtype="truefalseselect"></attribute>
    <attribute name="maxRedirects"></attribute>
    <attribute name="proxyPreferredAuthSchemes" size="60"></attribute>
    <attribute name="redirectsEnabled" inputtype="truefalseselect"></attribute>
    <attribute name="relativeRedirectsAllowed" inputtype="truefalseselect"></attribute>
    <attribute name="socketTimeout"></attribute>
    <attribute name="proxyHost"></attribute>
    <attribute name="proxyPort"></attribute>
    <attribute name="localAddress">
        <description><![CDATA[<p>如果不为空，使用InetAddress.getByName()方法获取。</p>
]]></description>
    </attribute>
    <attribute name="description" inputtype="html"></attribute>
</thing>