<?xml version="1.0" encoding="utf-8"?>

<Stage name="ThingEditorNode" _xmeta_id_="ThingEditor" descriptors="xworker.javafx.stage.Stage">
    <Nodes>
        <VBox name="mainBox" fillWidth="true" spacing="0">
            <Label name="titleLabel" text="标题"></Label>
            <MenuBar name="menuBar"></MenuBar>
            <ToolBar name="tooBar">
                <Button name="toolbarSaveButton" contentDisplay="GRAPHIC_ONLY" graphic="icons/save.gif"
                     prefWidth="16" prefHeight="16">
                    <style><![CDATA[.button {
    -fx-background-color: transparent, transparent, transparent, transparent;
}

.button:hover{
    -fx-background-color: transparent, rgba(0,0,0,.1), rgba(0,0,0,.1), transparent;
}

.button:armed {
    -fx-background-color: transparent, rgba(0,0,0,.1), rgba(0,0,0,.1), rgba(0,0,0,.2);
}]]></style>
                </Button>
                <Button name="toolbarDeleteButton" contentDisplay="GRAPHIC_ONLY">
                    <Graphic>
                        <ImageView name="ImageView" fitHeight="16" fitWidth="16" image="icons/delete.gif"></ImageView>
                    </Graphic>
                </Button>
                <Button name="toolbarMoveUpButton" graphic="icons/moveUp.gif"></Button>
                <Button name="toolbarMoveDownButton" graphic="icons/moveDown.gif"></Button>
                <Button name="toolbarFindButton">
                    <Graphic>
                        <ImageView name="ImageView" fitHeight="16" fitWidth="16" image="icons/search.gif"></ImageView>
                    </Graphic>
                </Button>
                <Separator name="Separator"></Separator>
                <Button name="classStructureButton" graphic="icons/descriptorExtends.gif"></Button>
                <Separator name="Separator" _xmeta_id_="Separator1"></Separator>
                <ChoiceBox name="descriptorChoiceBox">
                    <ThingStringConverter name="ThingStringConverter" descriptors="xworker.javafx.control.ControlStringValueConverters/@ThingStringConverter"></ThingStringConverter>
                </ChoiceBox>
                <MenuButton name="actionMenuButton" text="lang:d=动作&amp;en=Action"></MenuButton>
            </ToolBar>
            <SplitPane name="splitPane" dividerPositions="0.3,0.7">
                <TreeView name="outlineTree">
                    <ThingTreeCellFactory name="ThingTreeCellFactory" descriptors="xworker.javafx.control.cell.ThingTreeCellFactory"></ThingTreeCellFactory>
                    <ContextMenu name="outlineTreeContextMenu">
                        <MenuItem name="copyPathMenuItem" text="lang:d=拷贝路径&amp;en=Copy Path">
                            <MenuItemEventHandler name="onAction" methodName="copyPath"></MenuItemEventHandler>
                        </MenuItem>
                        <SeparatorMenuItem name="sp1"></SeparatorMenuItem>
                        <MenuItem name="moveUpMenuItem" text="lang:d=节点上移&amp;en=MoveUp">
                            <MenuItemEventHandler name="onAction" methodName="moveUp"></MenuItemEventHandler>
                        </MenuItem>
                        <MenuItem name="moveDownMenuItem" text="lang:d=节点下移&amp;en=MoveDown">
                            <MenuItemEventHandler name="onAction" methodName="moveDown"></MenuItemEventHandler>
                        </MenuItem>
                        <SeparatorMenuItem name="split2"></SeparatorMenuItem>
                        <MenuItem name="deleteMenuItem" text="lang:d=删除&amp;en=Delete">
                            <MenuItemEventHandler name="onAction" methodName="remove"></MenuItemEventHandler>
                        </MenuItem>
                        <MenuItem name="cutMenuItem" text="lang:d=剪切&amp;en=Cut">
                            <MenuItemEventHandler name="onAction" methodName="cut"></MenuItemEventHandler>
                        </MenuItem>
                        <MenuItem name="copyMenuItem" text="lang:d=复制&amp;en=Copy">
                            <MenuItemEventHandler name="onAction" methodName="copy"></MenuItemEventHandler>
                        </MenuItem>
                        <MenuItem name="pasteMenuItem" text="lang:d=粘贴&amp;en=Paste">
                            <MenuItemEventHandler name="onAction" methodName="paste"></MenuItemEventHandler>
                        </MenuItem>
                        <MenuItem name="pasteAsChildMenuItem" text="lang:d=粘贴-作为子节点&amp;en=Paste as Child">
                            <MenuItemEventHandler name="onAction" methodName="pasteAsChild"></MenuItemEventHandler>
                        </MenuItem>
                        <SeparatorMenuItem name="split3"></SeparatorMenuItem>
                        <MenuItem name="refreshMenuItem" text="lang:d=刷新&amp;en=Refresh">
                            <MenuItemEventHandler name="onAction" methodName="refresh"></MenuItemEventHandler>
                        </MenuItem>
                    </ContextMenu>
                </TreeView>
                <VBox name="contentBox" fillWidth="true"></VBox>
                <VBoxConstraints name="VBoxConstraints" vgrow="ALWAYS"></VBoxConstraints>
            </SplitPane>
        </VBox>
    </Nodes>
    <Scene name="Scene" root="var:mainBox"></Scene>
</Stage>