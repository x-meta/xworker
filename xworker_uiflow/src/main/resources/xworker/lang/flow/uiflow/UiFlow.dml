#$@text#$@
import xworker.lang.flow.uiflow.UiFlow;
import org.xmeta.ActionContext;

def ac = actionContext;

//创建uiFlow
def flowThing = self;
UiFlow uiFlow = new UiFlow(parent, flowThing, ac);

uiFlow.showOutlineDoc(self);
//选择流程节点
ac.tabFolder.setSelection(ac.flowItem);

//启动
if(self.getBoolean("startOnLoad")){
    uiFlow.start();
}

return uiFlow.getMainComposite();
