#$@text#$@
//创建容器面板
def containerThing = world.getThing("xworker.lang.flow.uiflow.nodes.base.prototypes.CompositeWithButton/@composite");
def ac = uiFlow.runComposite(self, containerThing);

//创建按钮
def buttons = self.doAction("getConnectionStarts", actionContext);
if(buttons != null){
    def bthing = world.getThing("xworker.lang.flow.uiflow.nodes.base.prototypes.CompositeWithButton/@4722/@4725");
    for(button in buttons){
        ac.peek().put("parent", ac.buttonComposite);
        def b = bthing.doAction("create", ac);
        b.setText(button.label);
        b.setData(button);
        //println("message butons=" + b);
    }
    ac.buttonComposite.getParent().layout();
}

def composite = self.doAction("getComposite", actionContext);
if(composite != null){
    //创建子内容
    ac.peek().put("parent", ac.contentComposite);
    composite.doAction("create", ac);
    ac.contentComposite.layout();
}else{
    uiFlow.log("Composite not found, node=" + self.getMetadata().getPath());
}
