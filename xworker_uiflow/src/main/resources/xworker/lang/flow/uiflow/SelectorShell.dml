#$@text#$@
import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.TableItem;

import xworker.util.XWorkerUtils;
import org.xmeta.Thing;

//默认显示选择器本身的文档
browser.setUrl(XWorkerUtils.getThingDescUrl(flowNode));

def cons = flowNode.getChilds("Connection");
//println("selectorshell flowNode=" + cons.size());
def context = [:];
for(l in cons){
    def name = l.getMetadata().getName();
    context.put(name, name);
    if(name.startsWith("_")){
        //前缀为_的不显示
        continue;
    }
    def item = new TableItem(table, SWT.NONE);
    item.setText([l.getMetadata().getName(), l.getMetadata().getLabel()] as String[]);
    item.setData(l);
}

def cases = flowNode.doAction("getConnectionStarts", parentContext);
for(ca in cases){
    if(context.get(ca.name) == null && !ca.name.startsWith("_")){
        def con = new Thing("xworker.lang.flow.uiflow.Connection");
        con.put("name", ca.name);
        con.put("label", ca.label);
        def item = new TableItem(table, SWT.NONE);
        item.setText([con.getMetadata().getName(), con.getMetadata().getLabel()] as String[]);
        item.setData(con)
    }
}
#$@text#$@
