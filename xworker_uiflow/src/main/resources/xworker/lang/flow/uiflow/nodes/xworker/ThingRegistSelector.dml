#$@text#$@
import xworker.util.UtilData;
import xworker.util.XWorkerUtils;

//显示面板
def composite = world.getThing("xworker.lang.flow.uiflow.nodes.xworker.prototypes.RegistSelectorShell/@composite");
def ac = uiFlow.runComposite(self, composite);

//初始化
def thing = self.doAction("getRegistThing", actionContext);
def type = self.doAction("getRegistType", actionContext);
ac.thingRegistSelector.doAction("init", ac, [thing:thing,type:type,keywords:null]);
