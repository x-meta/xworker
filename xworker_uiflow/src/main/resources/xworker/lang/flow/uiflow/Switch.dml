#$@text#$@
import xworker.util.UtilData;

def name = self.doAction("condition", actionContext);
def nextNode = null;
for(child in self.getChilds("Case")){
    if(child.getMetadata().getName() == name){
        nextNode = child;
        break;
    }
}
 
uiFlow.nodeFinished(self, nextNode);
