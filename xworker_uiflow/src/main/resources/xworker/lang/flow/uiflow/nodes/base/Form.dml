Scode
#$@text#$@
import xworker.util.UtilData;
import xworker.util.XWorkerUtils;

//显示面板
def composite = world.getThing("xworker.lang.flow.uiflow.nodes.base.prototypes.FormShell/@composite");
def ac = uiFlow.runComposite(self, composite);

//表单事物
def formThing = self.doAction("getFormThing", actionContext);
if(formThing != null){
    ac.form.doAction("setDescriptor", ac, ["descriptor":formThing]);
    
    //设置值
    def values = self.doAction("getInitValues", actionContext);
    if(values != null){
        ac.form.doAction("setValues", ac, ["values": values]);
    }
}
#$@text#$@
