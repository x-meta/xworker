Scode
#$@text#$@
//返回值
def file = self.doAction("run", actionContext);

//保存
self.doAction("saveValue", actionContext, ["value": file]);

//后续连接
if(file != null){
    uiFlow.nodeFinished(self, "OK");
}else{
    uiFlow.nodeFinished(self, "CANCEL");
}
