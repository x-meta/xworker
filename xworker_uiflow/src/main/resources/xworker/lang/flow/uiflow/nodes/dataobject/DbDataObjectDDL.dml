Scode
#$@text#$@
import org.xmeta.ActionContext;

//创建新的变量上下文
def ac = new ActionContext(actionContext);

//数据库数据对象
def dbDataObject = self.doAction("getDbDataObject", actionContext);

//指定DDL操作
dbDataObject.doAction("mapping2ddl", ac);

//节点已经结束，执行下级节点
uiFlow.nodeFinished(self, null);
