#$@text#$@
import xworker.util.UtilData;

def path = "";
if(UtilData.isTrue(self.doAction("condition", actionContext))){
    path = "Then@0";
}else{
    path = "Else@0";
}

def child = self.getThing(path);
def nextNode = null;
if(child != null){
    nextNode = world.getThing(child.getString("nodeRef"));
}
 
uiFlow.nodeFinished(self, nextNode);
