#$@text#$@
import org.xmeta.Thing;

def child = null;
if(linkStartInfo.name == "Then"){
    child = self.getThing("Then@0");
    if(child == null){
        child = new Thing("xworker.lang.flow.uiflow.If/@Then");
        self.addChild(child);
    }
}else{
    child = self.getThing("Else@0");
    if(child == null){
        child = new Thing("xworker.lang.flow.uiflow.If/@Else");
        self.addChild(child);
    }
}
child.put("nodeRef", target.getMetadata().getPath());
self.save();

uiFlow.recreateConnection(self);
