    //获取表单的值，通常返回的是Map
    def values = form.doAction("getValues", actionContext);
    
    //获取要保存到变量名
    def varForSave = flowNode.doAction("getVarForSave", parentContext);
    if(varForSave != null){
        parentContext.g().put(varForSave, values);
    }
    
    //执行下级节点
    uiFlow.nodeFinished(flowNode, "OK");
}
