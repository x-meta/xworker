#$@text#$@
import xworker.dataObject.DataObject;

def datas = [];
def dataObjectThing = world.getThing(dataObject);
for(attr in dataObjectThing.getChilds("attribute")){
    def data = new DataObject("xworker.app.view.swt.widgets.prototype.ImportByCreateOrUpdateShell/@keysTable/@dataStore/@dataObjects/@AbstractDataObject");
    data.put("name", attr.name);
    data.put("label", attr.metadata.label);
    datas.add(data);
}
return datas;
