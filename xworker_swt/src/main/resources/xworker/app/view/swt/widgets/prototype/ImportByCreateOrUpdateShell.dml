#$@text#$@
import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.MessageBox;

import org.xmeta.Thing;

//获取表单的输入
def values = thingForm.doAction("getValues", actionContext);
if(values.fileType == null || values.fileType == ""){
    def box = new MessageBox(shell, SWT.ICON_WARNING | SWT.OK | SWT.CANCEL);
    box.setText("导入数据");
    box.setMessage("请选择一个文件类型(fileType)！");
    box.open();
    return;
}
def keyFields = null;
for(item in keysTable.getItems()){
    if(item.getChecked()){
        if(keyFields == null){
            keyFields = item.getData().get("name");
        }else{
            keyFields = keyFields + "," + item.getData().get("name");
        }
    }
}
if(keyFields == null || keyFields == ""){
    def box = new MessageBox(shell, SWT.ICON_WARNING | SWT.OK | SWT.CANCEL);
    box.setText("导入数据");
    box.setMessage("关键字段(keyFields)不能为空！");
    box.open();
    return;
}

def box = new MessageBox(shell, SWT.ICON_WARNING | SWT.OK | SWT.CANCEL);
box.setText("导入数据");
box.setMessage("确实要导入数据么？");
if(SWT.OK == box.open()){
    //创建和执行导入动作
    def dataObjectThing = world.getThing(dataObject);
    
    def imp = new Thing("xworker.dataObject.actions.DataObjectActions/@importExcel");
    imp.put("label", "导入" + dataObjectThing.getMetadata().getLabel());
    imp.put("fileType", values.fileType);
    imp.put("keyFields", keyFields);
    imp.put("dataObject", dataObject);
    imp.put("file", file);
    
    //启动一个线程执行
    def actionContext = actionContext;
    new Thread({
        imp.getAction().run(actionContext);
    }).start();
    
    shell.dispose();
}
