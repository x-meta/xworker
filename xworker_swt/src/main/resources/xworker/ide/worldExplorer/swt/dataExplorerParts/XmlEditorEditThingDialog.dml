      sisSynchronized=false
      sthrowException=true
      suseOtherAction=false
      svarScope=Local
      sdisableGlobalContext=false
      Scode=
#$@text#$@
import org.xmeta.codes.XmlCoder;

if(desCombo.isDisposed()){
    return;
}

//读取编辑的数据
def values = thingForm.doAction("getValues", actionContext);
thing.getAttributes().putAll(values);
def desc = descriptors.get(desCombo.getSelectionIndex());
if(actionContext.get("cache") != null){
    for(key in cache.keySet()){
        if(key != desc.metadata.path){
           thing.getAttributes().putAll(cache.get(key));
        }
    }
}

//设置xml
def root = thing.getRoot();
    
//设置xml并重新定位光标到原来的位置
def xml = XmlCoder.encodeToString(root);
def offset = xmlText.getCaretOffset();
xmlText.setText(xml);
try{
    xmlText.setCaretOffset(offset);
}catch(Exception e){
}
