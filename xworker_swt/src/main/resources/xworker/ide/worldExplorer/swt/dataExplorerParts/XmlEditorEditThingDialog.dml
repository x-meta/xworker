#$@text#$@
//自动保存旧的
if(actionContext.g().get("cache") != null){
    actions.doAction("save", actionContext);
}
    
//初始化描述者列表 
def descriptors = thing.getDescriptors();
actionContext.g().put("descriptors", descriptors);
actionContext.g().put("thing", thing);
actionContext.g().put("oldIndex", 0);
actionContext.g().put("cache", [:]);
for(desc in descriptors){
    desCombo.add(desc.getMetadata().getLabel());
}
desCombo.select(0);

//默认编辑
thingForm.doAction("setThing", actionContext);
