#$@text#$@
import xworker.lang.executor.ExecuteRequestSwtUtils;

def prototype = world.getThing("xworker.lang.executor.requests.prototypes.ObjectViewer/@mainComposite");
//def composite = prototype.doAction("create", actionContext);
def composite = ExecuteRequestSwtUtils.createOkCancelControl(prototype, actionContext);

def object = self.doAction("getObject", actionContext);
objectViewer.doAction("setObject", actionContext, "object", object);

return composite;
