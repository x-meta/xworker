<?xml version="1.0" encoding="utf-8"?>

<Shell name="DefaultRequestService" descriptors="xworker.swt.widgets.Shell" text="lang:d=请求处理器&amp;en=Request Manager">
    <FillLayout name="FillLayout"></FillLayout>
    <Composite name="mainComposite" label="通知和请求" en_label="Notification &amp; Request" icon="icons/email.png">
        <FillLayout name="FillLayout"></FillLayout>
        <Resource name="resources">
            <Font name="boldFont" fontData="|10|1|#000000"></Font>
            <Color name="urgentColor" rgb="#FF8080"></Color>
        </Resource>
        <ThingLoader name="thingLoader" init="true" objectForLoadClass="xworker.lang.executor.SwtDefaultRequestService">
            <SashForm name="topSashForm" weights="30,70">
                <SashForm name="leftSashForm" style="VERTICAL" weights="60,40" BORDER="true">
                    <Table name="requestTable">
                        <DataStore name="DataStore" autoLoad="false" dataObject="xworker.lang.executor.dataobjects.Request"></DataStore>
                    </Table>
                    <Browser name="browser">
                        <UtilBrowser name="UtilBrowser"></UtilBrowser>
                    </Browser>
                </SashForm>
                <Composite name="contentComposite" BORDER="true">
                    <Composite name="defaultContent">
                        <FillLayout name="FillLayout"></FillLayout>
                        <ThingDescBrowser name="ThingDescBrowser">
                            <description><![CDATA[<h1>通知和请求</h1>

<p>&nbsp;&nbsp;&nbsp;&nbsp; XWorker的日志和请求框架下的默认请求处理服务。</p>

<p>&nbsp;&nbsp;&nbsp;</p>
]]></description>
                        </ThingDescBrowser>
                    </Composite>
                    <StackLayout name="StackLayout" topControl="defaultContent"></StackLayout>
                </Composite>
            </SashForm>
        </ThingLoader>
    </Composite>
    <Composite name="mainComposite" _xmeta_id_="mainComposite1" label="通知和请求-无ThingLoader"
         en_label="Notification &amp; Request" icon="icons/email.png">
        <FillLayout name="FillLayout"></FillLayout>
        <Resource name="resources">
            <Font name="boldFont" fontData="|10|1|#000000"></Font>
            <Color name="urgentColor" rgb="#FF8080"></Color>
        </Resource>
        <SashForm name="topSashForm" weights="30,70">
            <SashForm name="leftSashForm" style="VERTICAL" weights="60,40" BORDER="true">
                <Table name="requestTable">
                    <DataStore name="DataStore" autoLoad="false" dataObject="xworker.lang.executor.dataobjects.Request"></DataStore>
                </Table>
                <Browser name="browser">
                    <UtilBrowser name="UtilBrowser"></UtilBrowser>
                </Browser>
            </SashForm>
            <Composite name="contentComposite" BORDER="true">
                <Composite name="defaultContent">
                    <FillLayout name="FillLayout"></FillLayout>
                    <ThingDescBrowser name="ThingDescBrowser">
                        <description><![CDATA[<h1>通知和请求</h1>

<p>&nbsp;&nbsp;&nbsp;&nbsp; XWorker的日志和请求框架下的默认请求处理服务。</p>

<p>&nbsp;&nbsp;&nbsp;</p>
]]></description>
                    </ThingDescBrowser>
                </Composite>
                <StackLayout name="StackLayout" topControl="defaultContent"></StackLayout>
            </Composite>
        </SashForm>
    </Composite>
</Shell>