#$@text#$@
def openEditor = world.getAction("xworker.swt.app.treeMenuNodes.OpenEditor/@actions1/@run/@ActionDefined/@openEditor");
def editor = world.getThing("xworker.swt.app.editors.RegistThingEditor");
def thing = self.doAction("getThing", actionContext);
if(thing != null){
    openEditor(actionContext, "id", thing.getMetadata().getPath(), 
                              "editor", editor, 
                              "params",["thing": thing]);
}else{
    log.info("Thing not exists, path=" + self.getMetadata().getPath());
}
