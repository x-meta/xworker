        def params = ["thing": data];
        params.put(IEditor.EDITOR_THING, world.getThing("xworker.swt.app.editors.RegistThingEditor"));
        params.put(IEditor.EDITOR_ID, "ThingRegist:" + data.getMetadata().getPath());
        return params;
    }
}

