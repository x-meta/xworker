if(actionContext.get("title") != null){
    return title;
}

if(actionContext.get("src") != null){
    def file = new File(src);
    return file.getName();
}

if(actionContext.get("videoThing") != null){
    return videoThing.getMetadata().getLabel();
}

return "Video";
