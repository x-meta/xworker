import org.apache.commons.io.FileUtils;

if(dir.isDirectory()){
    FileUtils.deleteDirectory(dir);
}else{
    dir.delete();
}

//closeTabItem(treeItem);
treeItem.dispose();    

return dir;

def closeTabItem(treeItem){
    def s = treeItem.getData().get("dataId");
    def dir = new File(s);
    
    for(item in mainTabFolder.getItems()){
        def file = item.getData("__file__");
        if(dir.equals(file)){
            item.dispose();
            break;
        }
    }
    
    for(item in treeItem.getItems()){
        closeTabItem(item);
    }
}
