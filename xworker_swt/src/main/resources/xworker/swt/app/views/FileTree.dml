#$@text#$@
import org.xmeta.util.UtilFile;

def thingPath = UtilFile.getThingPathByFile(file);
if(thingPath != null){
    //文件是事物，编辑事物
    def thing = world.getThing(thingPath);
    openThingEditor(actionContext, "thing", thing);
}else{
    //是普通文件，用文件编辑器打开
    openFileEditor(actionContext, "file", file);
}
