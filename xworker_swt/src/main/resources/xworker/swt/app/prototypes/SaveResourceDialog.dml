  Scode=
#$@text#$@
import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.TableItem;

for(editor in parentContext.editorContainer.getEditors(true)){
    def item = new TableItem(editorsTable, SWT.NONE);
    item.setData(editor);
    item.setText([editor.getSimpleTitle(), editor.getTitle()] as String[]);
    item.setChecked(true);
}
