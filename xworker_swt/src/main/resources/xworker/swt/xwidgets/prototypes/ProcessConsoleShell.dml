      @/@mainTabFodler/@ActionContainer/@init
      sname=setProcess
      sisSynchronized=false
      sthrowException=true
      suseOtherAction=false
      svarScope=Local
      sdisableGlobalContext=false
      Scode=
#$@text#$@
//销毁旧的process
def oldProcess = actionContext.g().get("process");
if(oldProcess != null && thing.doAction("isDestroyOnDispose")){
   try{
       oldProcess.destroy();
   }catch(Exception e){
   }
}


//绑定输入流
bindInput(actionContext, "in", process.getErrorStream());
bindInput(actionContext, "in", process.getInputStream());

actionContext.g().put("process", process);

if("seperate" == style){
    actionContext.g().put("outputStream", process.getOutputStream());
}else{
    //绑定输出输出流，默认的single模式下
    bindToOutput(actionContext, "out", process.getOutputStream());
}
