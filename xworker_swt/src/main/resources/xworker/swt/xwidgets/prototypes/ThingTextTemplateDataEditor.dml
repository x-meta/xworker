#$@text#$@
import org.xmeta.util.ExceptionUtil;
import xworker.lang.actions.ActionUtils;


//保存和获取编辑的事物
thingEditor.doAction("save");
def thing = thingEditor.doAction("getThing");

//生成文本
try{
    def text = thing.doAction("run", parentContext.parentContext);
    if(text != null){
        editInput.setText(text);
    }else{
        editInput.setText("");
    }
}catch(Exception e){
    editInput.setText(ExceptionUtil.toString(e));
}

shell.dispose();
