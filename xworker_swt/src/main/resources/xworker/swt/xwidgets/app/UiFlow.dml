import xworker.lang.flow.uiflow.UiFlow;
import xworker.lang.flow.uiflow.CompositeFlow;
import org.xmeta.ActionContext;
import xworker.swt.design.Designer;
import xworker.swt.util.SwtUtils;

//不支持RAP
/*
if(SwtUtils.isRWT()){
    def thing = world.getThing("xworker.swt.xwidgets.app.prototypes.NotSupportRAP/@notSupportComposite");
    def ac = new ActionContext();
    ac.put("parent", parent);
    def composite = thing.doAction("create", ac);
    ac.put("parent", composite);
    for(child in self.getChilds()){
        child.doAction("create", actionContext);
    }

    return;
}*/

//使用新的变量上下文
def ac = new ActionContext();
ac.put("parentContext", actionContext);
ac.put("outlineBrowser", self.doAction("getOutlineBrowser", actionContext));
ac.put("descOutlineBrowser", self.doAction("getDescOutlineBrowser", actionContext));

//创建uiFlow
def flowThing = self.doAction("getUiFlow", actionContext);
def uiFlow = null;
Designer.pushCreator(self);
try{
   
    if(self.doAction("getType", actionContext) == "simple"){
        uiFlow = new CompositeFlow(parent, flowThing, ac);
    }else{
        uiFlow = new UiFlow(parent, flowThing, ac);
    }
    
    //创建子节点
    def composite = uiFlow.getMainComposite();
    actionContext.g().put(self.getMetadata().getName(), uiFlow);
    Designer.attach(composite, self.getMetadata().getPath(), actionContext);
    Designer.attachCreator(composite, self.getMetadata().getPath(), actionContext);   
}finally{
    Designer.popCreator();
}

actionContext.peek().put("parent", uiFlow.getMainComposite());
for(child in self.getChilds()){
    child.doAction("create", actionContext);
}

//启动
//println("autoStart=" + self.doAction("isAutoStart", actionContext));
if(self.doAction("isAutoStart", actionContext)){
    uiFlow.start();
}

return uiFlow.getMainComposite();
