#$@text#$@
import xworker.util.XWorkerUtils;
import org.xmeta.ActionContext;

def shell = XWorkerUtils.getIDEShell();
def ac = new ActionContext();
ac.put("parent", shell);
def menu = self.getThing("Menu@0");
if(menu == null){
    log.warn("must have an menu, thing=" + self.getMetadata().getPath());
    return;
}
ac.put("thingPath", menu.getMetadata().getPath());

def thing = null;
if(self.getBoolean("rootVisible", true)){
    thing = world.getThing("xworker.swt.xwidgets.app.SimpleAppWithSashFormShell/@composite");
}else{
    thing = world.getThing("xworker.swt.xwidgets.app.SimpleAppWithSashFormShell/@composite1");
}
def newShell = thing.doAction("create", ac);
newShell.setText(self.getMetadata().getLabel());

ac.actions.doAction("setContent", ac, [content: self, type:"thingDoc"]);
newShell.open();
