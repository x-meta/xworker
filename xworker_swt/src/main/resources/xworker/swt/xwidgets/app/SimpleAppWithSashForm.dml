#$@text#$@
import xworker.util.XWorkerUtils;

def type = self.getString("type");
def path = self.getStringBlankAsNull("path");
if(path == null){
    type = "thingDoc";
    path = self.getMetadata().getPath();
}
def url = null;
if(type == "url"){
    url = self.getString("path");
}else if(type == "webControl"){
    def thing = world.getThing(path);
    url = XWorkerUtils.getWebControlUrl(thing);
}else{
    url = XWorkerUtils.getThingDescUrl(path);
}

actions.doAction("setContent", actionContext, [type:"browser", content: url, thing: self]);
