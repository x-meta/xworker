#$@text#$@
import xworker.util.XWorkerUtils;

//创建组件
def useWebBrowser = self.getBoolean("useWebBrowser");
def thing = null;
if(useWebBrowser){
    thing = world.getThing("xworker.swt.xwidgets.app.SimpleAppWithTabFolderShell/@webBrowser");
}else{
    thing = world.getThing("xworker.swt.xwidgets.app.SimpleAppWithTabFolderShell/@browser");
}
    
thing.doAction("create", actionContext);

//获取和设置url
def url = self.doAction("getUrl", actionContext);
if(useWebBrowser){
    webBrowser.scripts.doAction("openUrl", actionContext, ["name":"default", "url": url]);
}else{
    browser.setUrl(url);
}
