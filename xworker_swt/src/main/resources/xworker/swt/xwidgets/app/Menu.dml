#$@text#$@
//创建根组件
def mainCompositeThing = world.getThing("xworker.swt.xwidgets.app.SimpleAppWithTabFolderShell/@composite");
def mainComposite = mainCompositeThing.doAction("create", actionContext);
actionContext.peek().put("parent", mainComposite);

//获取Composite
//print "'" + self.get("compositePath") + "'";
def composite = self.doAction("getComposite", actionContext);

if(composite == null){
    //显示文档
    log.warn("Composite is null, thing=" + self.getMetadata().getPath());
}else{
    //显示Composite
    composite.doAction("create", actionContext);
}
