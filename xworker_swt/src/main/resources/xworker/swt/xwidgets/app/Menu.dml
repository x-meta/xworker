//销毁之前的控件
def mainComposite = tabItem.getControl();
for(child in mainComposite.getChildren()){
    child.dispose();
}
actionContext.peek().put("parent", mainComposite);

//获取Composite
//print "'" + self.get("compositePath") + "'";
def composite = thing.doAction("getComposite", actionContext);

if(composite == null){
    //显示文档
    log.warn("Composite is null, thing=" + self.getMetadata().getPath());
}else{
    //显示Composite
    composite.doAction("create", actionContext);
}
