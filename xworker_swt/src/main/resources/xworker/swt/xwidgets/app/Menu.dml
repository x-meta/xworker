#$@text#$@
import org.xmeta.Thing;

//获取dataObject
def dataObject = self.doAction("getDataObject", actionContext);

if(dataObject == null){
    //显示文档
    log.warn("DataObject is null, thing=" + self.getMetadata().getPath());
}else{
    //显示演示
    def demo = new Thing("xworker.swt.xwidgets.DemoDataObject");
    demo.set("name", "demo");
    demo.set("dataObjectPath", dataObject.getMetadata().getPath());
    demo.doAction("create", actionContext);
}
