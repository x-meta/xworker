#$@text#$@
def parentComposite = self.doAction("getParentComposite", actionContext);
def topControl = self.doAction("getTopControl", actionContext);
def stackLayout = self.doAction("getStackLayout", actionContext);

stackLayout.topControl = topControl;
parentComposite.layout();
