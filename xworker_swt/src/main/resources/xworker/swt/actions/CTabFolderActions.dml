#$@text#$@
import org.xmeta.util.UtilFile;

def file = self.doAction("getFile", actionContext);
def tabFolder = self.doAction("getCTabFolder", actionContext);
//println("file=" + file);
if(file == null){
    log.warn("file is null, ation=" + self.getMetadata().getPath());
    return;
}

def thingPath = UtilFile.getThingPathByFile(file);

//println("tabFolder=" + tabFolder);
//println("cTabFolder=" + self.getString("cTabFolder"));
//println("sxx=" + actionContext.get("tabFolder"));

def tabItem = null;
if(thingPath != null){
    def thing = world.getThing(thingPath);
    tabItem = self.doAction("openThingEditor", actionContext, 
        [thing: thing, "tabFolder": tabFolder,  "openFileThing": self]);
}else{
    tabItem = self.doAction("openTextFileEditor", actionContext,
         [file: file, "tabFolder": tabFolder, "openFileThing": self]);
}
if(tabItem != null){
    tabItem.setData("__file__", file);
}
