#$@text#$@
import xworker.util.XWorkerUtils;

def useWebBrowser = self.doAction("isUseWebBrowser", parentContext);

def type = self.doAction("getType", parentContext);
def path = self.doAction("getPath", parentContext);
//println("self.path=" + self.getString("path"));
//println("parentContext.path=" + parentContext.get("url"));
if(path == null){
    type = "thingDoc";
    path = self.getMetadata().getPath();
}
def url = null;
if(type == "url"){
    url = path;
}else if(type == "webControl"){
    thing = world.getThing(path);
    url = XWorkerUtils.getWebControlUrl(thing);
}else{
    url = XWorkerUtils.getThingDescUrl(path);
}

//println(url);
if(useWebBrowser){
    webBrowser.scripts.doAction("openUrl", actionContext, ["name":"default", "url": url]);
}else{
    browser.setUrl(url);
}
