#$@text#$@
import org.eclipse.swt.SWT;
import org.eclipse.swt.custom.CTabItem;
import org.xmeta.ActionContext;
import org.xmeta.ActionException;
import xworker.util.XWorkerUtils;

//TabItem的键值
def tabItemKey = self.doAction("getTabItemKey", actionContext);
//内容的键值
def contentKey = self.doAction("getTabContentKey", actionContext);

//获取CTabFolder
def tabFolder = self.doAction("getCTabFolder", actionContext);
if(tabFolder == null){
    throw new ActionException("CTabFolder is null, action=" + self.getMetadata().getPath());
}
def variables = self.doAction("getVariables", actionContext)
//println self.tabItemKey;
//println actionContext.get("thing");
//println "tabItemKey=" + tabItemKey;
//println "contentKey=" + contentKey;

//查看是否已经打开
def opened = false;
def tabItem = null;
for(item in tabFolder.getItems()){
    def key = item.getData("tabItemKey");
    def ckey = item.getData("tabContentKey");
    
    if(tabItemKey == key && contentKey == ckey){
        opened = true;
        tabFolder.setSelection(item);
        return;
    }else if(tabItemKey == key){
        tabItem = item;
        break;
    }
}

//是否需要创建
if(tabItem == null){
    int style = SWT.NONE;
    if(self.doAction("isClose")){
        style = style | SWT.CLOSE;
    }
    
    tabItem = new CTabItem(tabFolder, style);
    def ac = new ActionContext();
    if(variables != null){
        ac.putAll(variables);
    }
    ac.put("parentContext", actionContext);
    ac.put("parent", tabFolder);
    ac.put("tabFolder", tabFolder);
    ac.put("tabItem", tabItem);
    def outlineBrowser = self.doAction("getOutlineBrowser", actionContext);
    ac.put("outlineBrowser", outlineBrowser);    
    
    def compositeThing = world.getThing("xworker.swt.xwidgets.app.SimpleAppWithTabFolderShell/@composite");
    def composite = compositeThing.doAction("create", ac);
    tabItem.setControl(composite);

    tabItem.setData("actionContext", ac);
    tabItem.setData("outlineBrowser", outlineBrowser);
    //对应事物创建内容
    ac.put("parent", composite);    
    self.doAction("create", ac);
}

//初始化控件
def textLabel = self.doAction("getTabItemText", actionContext);
//println "textLabel=" + textLabel + ":" + self.tabItemText;
if(textLabel == null){
    textLabel = self.metadata.label;
}

tabItem.setData("tabItemKey", tabItemKey);
tabItem.setData("tabContentKey", contentKey);
tabItem.setText(textLabel);

def ac = tabItem.getData("actionContext");
ac.put("parentContext", actionContext);
self.doAction("initTabItem", ac);

def composite = tabItem.getControl();
composite.layout();

tabFolder.setSelection(tabItem);

//println("CTabFodlerActions===" + self.metadata.path);
composite.getShell().setVisible(true);

return tabItem;
