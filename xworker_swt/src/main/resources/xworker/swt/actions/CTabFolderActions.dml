      Scode
#$@text#$@
import xworker.swt.events.SwtListener;
import org.xmeta.Thing;
import org.xmeta.Action;
import xworker.lang.actions.ActionContainer;

def tabFolder = self.doAction("getCTabFolder", actionContext);

for(tabItem in tabFolder.getItems()){
    def ac = tabItem.getData("actionContext");
    if(ac != null){
        def okButtonSelection = ac.get("okButtonSelection");
        if(okButtonSelection != null){
            if(okButtonSelection instanceof SwtListener){
                okButtonSelection.handleEvent(null);
            }else if(okButtonSelection instanceof Thing){
                okButtonSelection.getAction().run(ac);
            }else if(okButtonSelection instanceof Action){
                okButtonSelection.run(ac);
            }else if(okButtonSelection instanceof ActionContainer){
                okButtonSelection.doAction("save", ac);
            }
        }
    }
}
