#$@text#$@
import org.xmeta.ActionContext;

def shell = self.doAction("getShell", actionContext);

def thing = self.doAction("getThing", actionContext);
if(thing == null){
    return;
}

def ac = new ActionContext();
ac.put("parent", shell);
ac.put("parentContext", actionContext);
ac.put("actionThing", self);

def dialogThing = world.getThing("xworker.ide.worldexplorer.swt.tools.SaveAsDialog");
def newShell = dialogThing.doAction("create", ac);
newShell.setVisible(true);
