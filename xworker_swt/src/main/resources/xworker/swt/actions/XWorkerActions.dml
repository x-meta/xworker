#$@text#$@
import org.xmeta.ActionContext;

//用于打开窗口的shell
def shell = self.doAction("getShell", actionContext);

//索引事物
def indexThing = self.doAction("getIndex", actionContext);

//索引类型
def type = self.doAction("getType", actionContext);

//创建新事物的描述者
def descriptor = self.doAction("getDescriptorForNewThing", actionContext);

def ac = new ActionContext();
ac.put("parent", shell);
ac.put("parentContext", actionContext);
ac.put("actionThing", self);

def dialogThing = world.getThing("xworker.swt.actions.prototypes.FavoriteDialog");
def dialogShell = dialogThing.doAction("create", ac);
dialogShell.setVisible(true);

ac.thingRegistor.doAction("init", ac, "thing", indexThing, "type", type, 
        "keywords", "", "descritporForNewThing", descriptor);
ac.thingRegistor.doAction("refresh", ac);
