#$@text#$@
import org.xmeta.ActionException;

//获取编辑器和事物
def thing = self.doAction("getThing", actionContext);
def thingEditor = self.doAction("getThingEditor", actionContext);

if(thing == null || thingEditor  == null){
    throw new ActionException("thing or thingeditor is null, action=" + self.getMetadata().getPath());    
}

//编辑器设置事物
thingEditor.editorActions.doAction("setThing", ["thing":thing]);
