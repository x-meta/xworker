import org.xmeta.ActionContext;

//log.info("self=" + self);
//初始化变量等
def type = self.getString("type");
self.doAction("init", actionContext);
//把自己也保存到全局变量上下文中
actionContext.g().put("thingSelf", self);

def content = actionContext.get("content");
//log.info("type:" + type + "，　content:" + content);

//如果没有内容显示一个空白
if(content == null){
    def thing = world.getThing("xworker.swt.xworker.ThingRegistThing/@composite/@noContent");
    return thing.doAction("create", actionContext);
}else{
    actionContext.g().put("content", content);
    self.doAction(type, actionContext, ["content": content, "execute":false]);
}
