#$@text#$@
//值是单行字符串的
def stringType = ["url"];
//值是self的
def selfType = ["groovy", "code", "html"];
//值是regsit的
def registType = ["thingRegist"];
//如果content=null，那么使用默认自己的
def nullSelf = ["thingDesc"];

//获取内容
def content = null;
def type = self.getString("type");
if(isType(type, stringType)){
    //值是单行字符串类型的
    content = self.doAction("getString", actionContext);
}else if(isType(type, selfType)){
    //值是自己的
    content = self;
}else if(isType(type, registType)){
    //值是注册类型的
    content = self.getStringBlankAsNull("path");
    if(content == null){
        return;
    }        
    def ps = content.split("[|]");
    if(ps.length < 2){
        return;
    }
    actionContext.g().put("registType", ps[0]);
    content = world.getThing(ps[1]);
    if(ps.length >= 3){
        actionContext.g().put("descriptor", ps[2]);
        if(ps.length >= 4){
            actionContext.g().put("contentDefaultOpenMethod", ps[3]);
        }else{
            actionContext.g().put("contentDefaultOpenMethod", null);
        }
        if(ps.length >= 5){
            actionContext.g().put("contentDisplayType", ps[4]);  
        }else{
            actionContext.g().put("contentDisplayType", "Composite");      
        }
    }else{
        if(content != null && content.getDescriptor().getMetadata().getPath() == "xworker.lang.util.ThingIndex"){
            //如果事物是ThingIndex，使用ThingIndex自身的设置
            log.info("contentDisplayType=" + content.get("contentDisplayType"));
            actionContext.g().put("descriptor", content.get("descritporForNewThing"));
            actionContext.g().put("contentDefaultOpenMethod", content.get("contentDefaultOpenMethod"));
            actionContext.g().put("contentDisplayType", content.get("contentDisplayType"));            
        }else{
            actionContext.g().put("descriptor", null);
            actionContext.g().put("contentDefaultOpenMethod", "thingDesc");
            actionContext.g().put("contentDisplayType", "Composite");            
        }
    }
}else{
    //默认都是事物类型
    content = self.doAction("getThing", actionContext);
    //取第一个子事物
    if(content == null && self.getChilds().size() > 0){
        content = self.getChilds().get(0);
    }
}

if(content == null && isType(type, nullSelf)){
    //如果content为null，那么默认使用self的
    content = self;
}

actionContext.g().put("content", content);

def isType(type, types){
    for(t in types){
        if(type == t){
            return true;
        }
    }
    
    return false;
}
