#$@text#$@
import ognl.Ognl;
import xworker.util.XWorkerUtils;

if(execute){
    //content.getAction().run(actionContext);
}else{
    def thing = world.getThing("xworker.swt.xworker.ThingRegistThing/@composite/@codeComposite");
    
    def composite = thing.doAction("create", actionContext);  
    
    //代码着色
    def codeType = content.getString("codeType");    
    def colorer = world.getThing("xworker.swt.xworker.ThingRegistThing/@composite/@groovyComposite/@mainSashForm/@codeComposite/@codeText/@Colorer").detach();
    colorer.put("codeType", codeType);
    colorer.put("codeName", codeType);
    actionContext.peek().put("parent", codeText);
    colorer.doAction("create", actionContext);
    
    def code =  content.getString("code");
    if(code == null){
        code = "";
    }
    codeText.setText(code); 
    
    try{
        def styledTextForInsert = getStyledTextForInsert(actionContext, "styledTextForInsert", content.getString("styledTextForInsert"));
        if(styledTextForInsert == null){
            styledTextForInsert = Ognl.getValue(content.getString("styledTextForInsert"), actionContext);
        }
        if(styledTextForInsert != null){
            actionContext.g().put("styledTextForInsert", styledTextForInsert);
        }else{
            insertButton.dispose();
        }
    }catch(Exception e){
        log.warn("get styledTextForInsert error", e);
        insertButton.dispose();
    }
    
    return composite;
}
