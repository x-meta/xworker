#$@text#$@
import org.xmeta.ActionContext;

//变量
def type = self.doAction("getType", actionContext);
def userTask = self.doAction("getUserTask", actionContext);
def shell = self.doAction("getShell", actionContext);

//创建对话框
def ac = new ActionContext();
ac.put("parent", shell);
ac.put("userTask", userTask);

def dialogThing = world.getThing("xworker.swt.xworker.dialogs.prototype.UserTaskTextDialog");
def newShell = dialogThing.doAction("create", ac);
newShell.setVisible(true);
