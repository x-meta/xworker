    svarScope=Local
    sdisableGlobalContext=false
    Scode=
#$@text#$@
import org.xmeta.ActionContext;

//创建控件
def prototype = world.getThing("xworker.swt.xworker.prototype.DescriptorSelectorShell/@thingsCompostie");
def ac = new ActionContext();
ac.put("parent", parent);
ac.put("instance", self);
ac.put("instanceActionContext", actionContext);
def composite = prototype.doAction("create", ac);

//执行子节点的create方法，一般是创建布局等
actionContext.peek().put("parent", composite);
for(child in self.getChilds()){
    child.doAction("create", actionContext);
}

//保存变量
actionContext.g().put(self.metadata.name, ac.get("actions"));
return composite;
