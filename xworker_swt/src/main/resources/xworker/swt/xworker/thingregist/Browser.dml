#$@text#$@
import xworker.command.Selector;
import xworker.util.XWorkerUtils;

//创建选择面板
def compositeThing = world.getThing("xworker.command.selectors.BrowserSelector/@composite");
def composite = compositeThing.doAction("create", actionContext);

//初始化注册选择
def url = self.doAction("getUrl", actionContext);
browser.scripts.doAction("openUrl", actionContext, ["name":"default", "url": url]);
actionContext.g().put("url", url);

return composite;
