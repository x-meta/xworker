    Scode=
#$@text#$@
def composite = self.doAction("getComposite", actionContext);

//创建概要树的浏览器
if(actionContext.get("outlineComposite") != null && composite != null){
    actionContext.peek().put("parent", outlineComposite);
    def com = composite.doAction("create", actionContext);
    if(actionContext.get("tab") != null){
        tab.setData("outlineTree", com);
    }
}
