#$@text#$@
import xworker.swt.design.InjectableComposite;

import org.xmeta.Thing;

def itemNames = ["ExpandItem", "CTabItem", "TabItem"];
//获取正编辑的事物
def thing = thingEditor.editorActions.doAction("getRootThing");
for(itemName in itemNames){
    if(thing.getThingName() == itemName){
        def icomposite = new Thing("xworker.swt.design.InjectableComposite");
        thing.addChild(icomposite);
    }
}

//获取ui
def ui = parentContext.parentContext.parentContext.ui;

//可注入的面板对应的事物
def uithing = ui.getControl().getData("thing");

//获取子节点
def child = uithing.getThing("Control@0");
if(child != null && child.getChilds().size() > 0){
    child = child.getChilds().get(0);
}else{
    child = null;
}

if(child == null){
    return;
}

child.addChild(thing);

//设置子节点，在帮助小精灵的变量上下文中
InjectableComposite.setChild(child, parentContext);

shell.dispose();
#$@text#$@
