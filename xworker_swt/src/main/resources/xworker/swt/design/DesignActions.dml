    @/@Init/@actions
    sname=actions
    sdescriptors=xworker.lang.MetaDescriptor3/@actions
    sinheritDescription=false
    sth_createIndex=false
    sth_registMyChilds=false
    sth_registDisabled=false
    sth_mark=false
    sid=actions
      @/@Init/@actions/@run
      sname=run
      sisSynchronized=false
      sthrowException=true
      suseOtherAction=false
      svarScope=Local
      sdisableGlobalContext=false
      Scode=
#$@text#$@
import xworker.swt.design.DesignTools;

//注册toolActions
def actions = self.doAction("getActionContainer", actionContext);

parentContext.g().put("toolActions", actions);

return actions.doAction("handleNewControl", actionContext, "newControl", parentContext.control);
