    Scode=
#$@text#$@
//演示和控件
def control = self.doAction("getControl", actionContext);
def demo = self.doAction("getDemo", actionContext);
if(demo == null){
    demo = control;
}

actionContext.g().put("injectableControl", self);

//显示面板
def thing = world.getThing("xworker.swt.design.prototype.InjectableControlShell/@mainComposite");
def composite = thing.doAction("create", actionContext);

//显示demo
actionContext.peek().put("parent", demoComposite);
demo.doAction("create", actionContext);
composite.getParent().layout(true);
//demoComposite.getParent().layout(true);

return composite;
