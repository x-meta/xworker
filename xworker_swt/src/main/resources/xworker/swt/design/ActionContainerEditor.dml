    Scode=
#$@text#$@
import org.xmeta.Thing;

//获取可注入面板
def ui = parentContext.parentContext.ui;
def control = ui.getControl();
def thing = control.getData("thing");

def childThing = thing.getThing("ActionContainer@0");
if(childThing == null){
    childThing = new Thing("xworker.swt.design.InjectableComposite/@ActionContainer");
    childThing.set("name", "actions");
}

thingEditor.doAction("setThing", actionContext, "thing", childThing);
actionContext.g().put("actionContainerThing", childThing);
#$@text#$@
