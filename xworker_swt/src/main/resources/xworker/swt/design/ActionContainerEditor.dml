#$@text#$@
import xworker.swt.design.InjectableComposite;

//获取正编辑的事物
thingEditor.doAction("save");
def ui = parentContext.parentContext.ui;
def control = ui.getControl();
def thing = control.getData("thing");

def childThing = thing.getThing("ActionContainer@0");
if(childThing == null){
    thing.addChild(actionContainerThing);
    thing.save();
}

actionContainerThing.doAction("create", ui.getActionContext());
#$@text#$@
