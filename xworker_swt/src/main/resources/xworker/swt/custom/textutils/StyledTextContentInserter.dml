if(value != null && actionContext.get("text") != null){
    def offset = text.getCaretOffset();
    text.insert(value);
    if(text.getCaretOffset() == offset){
        text.setCaretOffset(offset + value.length())
    }
}

actionContext.g().put("result", value);
shell.dispose();

if(actionContext.get("text") == null){
    return;
}
text.display.asyncExec({
    text.setFocus();
});
