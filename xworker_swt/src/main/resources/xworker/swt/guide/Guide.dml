#$@text#$@
import xworker.util.XWorkerUtils;

//类型
def type = self.doAction("getType", actionContext);
def url = null;
switch(type){
    case "url":
        url = self.doAction("getUrl", actionContext);    
        break;
    case "webControl":
        def control = self.doAction("getWebControl", actionContext);    
        url = XWorkerUtils.getWebControlUrl(control);
        break;
    default:
        def thing = self.doAction("getThing", actionContext);
        if(thing == null){
            thing = self;
        }
        url = XWorkerUtils.getThingDescUrl(thing);
}

if(self.getBoolean("simpleBrowser")){
    //创建浏览器
    def browserThing = world.getThing("xworker.swt.guide.prototypes.SimpleBrowser/@browser");
    browserThing.doAction("create", actionContext);
    
    //打开网页
    browser.setUrl(url);
}else{
    //创建浏览器
    def browserThing = world.getThing("xworker.swt.guide.prototypes.Browser/@webBrowser");
    browserThing.doAction("create", actionContext);
    
    //打开网页
    webBrowser.scripts.doAction("openUrl", actionContext, ["name":self.metadata.label,
         "url":url]);
}
