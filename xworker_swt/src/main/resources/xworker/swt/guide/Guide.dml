#$@text#$@
import xworker.util.XWorkerUtils;
import xworker.swt.util.SwtUtils;

//网页类型
def type = self.doAction("getType", actionContext);
def url = null;
switch(type){
    case "url":
        url = self.doAction("getUrl", actionContext);    
        break;
    case "webControl":
        def control = self.doAction("getWebControl", actionContext);    
        url = XWorkerUtils.getWebControlUrl(control);
        break;
    default:
        def thing = self.doAction("getThing", actionContext);
        if(thing == null){
            thing = self;
        }
        url = XWorkerUtils.getThingDescUrl(thing);
}

def composite = null;
def bl = self.getString("browserLocation");
switch(bl){
    case "left":
        composite = world.getThing("xworker.swt.guide.prototypes.BrowserComposite/@leftRight");    
        break;
    case "top":
        composite = world.getThing("xworker.swt.guide.prototypes.BrowserComposite/@leftRight1");    
        break;
    case "right":
        composite = world.getThing("xworker.swt.guide.prototypes.BrowserComposite/@leftRight2");    
        break;
    case "topicBrowser":
        composite = world.getThing("xworker.swt.guide.prototypes.BrowserComposite/@leftRight3/@composite");    
        break; 
    default:
        composite = world.getThing("xworker.swt.guide.prototypes.BrowserComposite/@leftRight3");    
        break;        
}
if(self.getStringBlankAsNull("weights") != null){
    composite = composite.detach();
    composite.set("weights", self.get("weights"));
}

//创建面板和浏览器
composite.doAction("create", actionContext);
//设置网页内容
if(bl == "topicBrowser"){
    //在XWorker主页下的向导通过parentConext可以获取到topicBrowser
    if(topicBrowser != null){
        topicBrowser.setUrl(url);
    }
}else{
    browser.setUrl(url);
}

//创建Composite
composite = self.doAction("getComposite", actionContext);
if(composite == null){
    composite = world.getThing("xworker.swt.guide.prototypes.NoContent/@browser");
}
actionContext.peek().put("parent", contentComposite);
composite.doAction("create", actionContext);
