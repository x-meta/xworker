<?xml version="1.0" encoding="utf-8"?>

<thing name="StringHandler" descriptors="xworker.lang.MetaDescriptor3">
    <description><![CDATA[<p>处理String消息，触发onMessage事件。</p>
]]></description>
    <actions>
        <JavaAction name="create" outerClassName="xworker.io.netty.handlers.StringHandler"
             methodName="create"></JavaAction>
        <NOOP name="onMessage" usage="event">
            <ins>
                <param name="ctx" type="io.netty.channel.ChannelHandlerContext"></param>
                <param name="msg" type="java.lang.String"></param>
            </ins>
        </NOOP>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="description" inputtype="html"></attribute>
</thing>