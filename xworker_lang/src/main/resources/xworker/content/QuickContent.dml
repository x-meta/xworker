        <value name="thingRegist" value="thingRegist"/>&#xd;
        <value name="composite" value="composite"/>&#xd;
        <value name="shell" value="shell"/>&#xd;
        <value name="uiFlow" value="uiFlow"/>&#xd;
        <value name="uiFunction" value="uiFunction"/>&#xd;
        <value name="autoDemo" value="autoDemo"/>&#xd;
        <value name="swtDemo" value="swtDemo"/>&#xd;
        <value name="swtGuide" value="swtGuide"/>&#xd;
        <value name="webDemo" value="webDemo"/>&#xd;
        <value name="thingDemo" value="thingDemo"/>&#xd;
        <value name="action" value="action"/>&#xd;
        <value name="groovy" value="groovy"/>&#xd;
        <value name="dataObjectEditor" value="dataObjectEditor"/>&#xd;
        <value name="dataObjectSelector" value="dataObjectSelector"/>&#xd;
        <value name="dataObjectMultiSelector" value="dataObjectMultiSelector"/>&#xd;
        <value name="code" value="code"/>&#xd;
