#$@text#$@
import xworker.task.UserTaskManager;

//检查
def userTask = UserTaskManager.createTask(self, false);
userTask.start();
try{
    userTask.setCurrentLabel("Grabbing...");
    def grabers = self.doAction("getGrabers", actionContext);
    //println(grabers);
    if(grabers != null){
        for(graber in grabers){
            //println(graber);
            graber.doAction("run", actionContext, "graberManager", self, 
                            "userTask", userTask);
        }
    }
}finally{
    userTask.finished();
}
