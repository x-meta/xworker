#$@text#$@
import xworker.util.XWorkerUtils;

if(actionContext.get("outlineBrowser") != null){
    def ac = dataObjectEditor.getData("actionContext");
    def data = ac.dataTable.getSelection()[0].getData();
    def content = data.getString("actionPath");
    def thing = world.getThing(content);
    if(thing != null){
        outlineBrowser.setUrl(XWorkerUtils.getThingDescUrl(thing));
    }else{
        outlineBrowser.setText("动作不存在");
    }
}
