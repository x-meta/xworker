#$@text#$@
import org.xmeta.Thing;

def ds = world.getThing("_local.xworker.db.XWorkerAppDataSource");
if(ds == null){
    def dir = new File("${world.path}/databases/sqlite");
    if(dir.exists() == false){
        dir.mkdirs();
    }
    //如果数据源不存在，创建数据源
    ds = new Thing("xworker.db.jdbc.DataSource");
    ds.put("name", "XWorkerAppDataSource");
    ds.put("type", "HikariCP");
    ds.put("dbType", "sqlite");
    ds.put("drivers", "15");
    ds.put("driver_class", "org.sqlite.JDBC");
    ds.put("url", "template:jdbc:sqlite:\${world.path}/databases/sqlite/xworker_app.db");
    ds.put("checkoutTimeout", "3000");
    ds.getMetadata().setCoderType("dml_xml");
    ds.saveAs("_local", "_local.xworker.db.XWorkerAppDataSource");    
}
