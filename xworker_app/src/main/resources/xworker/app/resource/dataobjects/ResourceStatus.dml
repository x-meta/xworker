#$@text#$@
//返回级别对应的颜色
def colors = [
    "muted": "#FFFFFF",
    "primary":"#0080C0",
    "success":"#D5FFD5",
    "info":"#BBFFFF",
    "warning":"#FFFFD5",
    "danger":"#FFD9D9"
];

def color = colors.get(theData.getString("level"));
if(color != null){
    return color;
}else{
    return "#000000";
}
