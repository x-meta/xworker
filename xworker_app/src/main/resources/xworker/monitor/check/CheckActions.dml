  <tr>
  <#list config.getChilds("Field") as field>
      <th>${field.metadata.label}</th>
  </#list>    
  </tr>
  <#list messages as msg>
  <tr>
      <#list config.getChilds("Field") as field>
      <td>${msg[field.attributePath]?if_exists}</td>
      </#list>
  </tr>
  </#list>
