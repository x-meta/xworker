    @/@actions/@isBreak
    sname=isBreak
    sisSynchronized=false
    sthrowException=true
    suseOtherAction=false
    svarScope=Local
    sdisableGlobalContext=false
    Scode=
#$@text#$@
//默认系统循环没有规则执行，且breakOnNoRules属性值为true
if(!hasRule && system.getBoolean("breakOnNoRules")){
    return true;
}else{
    return false;
}
