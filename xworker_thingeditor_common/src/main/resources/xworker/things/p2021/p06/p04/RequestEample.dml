<?xml version="1.0" encoding="utf-8"?>

<RequestIndex name="RequestEample" descriptors="xworker.lang.executor.RequestIndex">
    <ThingForm validate="true" priority="urgent" timeout="900000">
        <Thing>
            <attribute name="name" validateAllowBlank="false"></attribute>
            <attribute name="label"></attribute>
        </Thing>
        <actions>
            <GroovyAction name="onOk" descriptors="xworker.lang.actions.GroovyAction" code="println &quot;ok:&quot; + values;"></GroovyAction>
            <GroovyAction name="onCancel" descriptors="xworker.lang.actions.GroovyAction" code="println &quot;cancel&quot;;"></GroovyAction>
            <GroovyAction name="onValidateFalse" descriptors="xworker.lang.actions.GroovyAction">
                <code><![CDATA[println "validate false";

return false;]]></code>
            </GroovyAction>
        </actions>
    </ThingForm>
    <Url name="Url" url="https://www.xworker.org"></Url>
    <Html name="Html" html="&lt;h1&gt;Hello World!&lt;h1/&gt;"></Html>
    <ThingDesc name="ThingDesc">
        <description><![CDATA[<h1>Hello World ThingDesc.</h1>
]]></description>
    </ThingDesc>
</RequestIndex>