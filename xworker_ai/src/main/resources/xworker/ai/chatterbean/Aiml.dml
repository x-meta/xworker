def xml = ident + "<category>\n";
def pattern = self.getStringBlankAsNull("label");
def template = self.getStringBlankAsNull("template");
if(pattern == null || template == null){
    return;
}

pattern = pattern.toUpperCase();
//pattern
xml = xml + ident + "  <pattern>" + pattern + "</pattern>\n";
//that
def that = self.getStringBlankAsNull("that");
if(that != null){
    xml = xml + ident + "  <that>" + that + "</that>\n";
}
//template
//if(template.split("[\n]").length > 1){
//    xml = xml + ident + "  <template><![CDATA[" + template + "]]></template>\n";
//}else{
    xml = xml + ident + "  <template>" + template + "</template>\n";
//}
xml = xml + ident + "</category>";

return xml;
#$@text#$@
