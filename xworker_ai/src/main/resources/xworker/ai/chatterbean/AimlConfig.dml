Scode
#$@text#$@
//bot学习这些知识，参数bot
learn(bot, world.getThing(self.getString("xworker")));
learn(bot, world.getThing(self.getString("local")));
learn(bot, world.getThing(self.getString("work")));

def others = self.getStringBlankAsNull("otherAimls");
if(others != null){
    for(other in others.split("[\n]")){
        learn(bot, world.getThing(other.trim()));
    }
}

def learn(bot, thing){
    if(thing != null){
        thing.doAction("learn", actionContext, ["bot":bot]);
    }
}
