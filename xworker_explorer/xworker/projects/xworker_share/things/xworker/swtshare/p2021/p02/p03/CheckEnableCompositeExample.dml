<?xml version="1.0" encoding="utf-8"?>

<QuickFunction name="CheckEnableCompositeExample" descriptors="xworker.swt.xworker.ThingRegistThing"
     label="CheckEnableComposite" type="swtDemo" group="xworker.-800|SWT.-1000|widgets.xwidgets"
     icon="icons/application.png" th_createIndex="true" th_registThing="child|xworker.example.ExamplesIndex">
    <Composite>
        <GridLayout name="GridLayout"></GridLayout>
        <CheckEnableComposite name="checkText">
            <ContentControl>
                <Text name="text"></Text>
            </ContentControl>
            <GridData name="GridData" style="FILL_HORIZONTAL"></GridData>
        </CheckEnableComposite>
        <CheckEnableComposite name="checkCombo">
            <ContentControl>
                <Combo>
                    <value name="mael" label="男" value="0"></value>
                    <value name="fmale" label="女" value="1"></value>
                </Combo>
            </ContentControl>
        </CheckEnableComposite>
        <CheckEnableComposite name="checkCCombo">
            <ContentControl>
                <CCombo BORDER="false">
                    <value name="男" value="0"></value>
                    <value name="女" value="1"></value>
                </CCombo>
            </ContentControl>
        </CheckEnableComposite>
        <CheckEnableComposite name="checkDatePicker">
            <ContentControl>
                <DatePickerCombo name="datePicker"></DatePickerCombo>
            </ContentControl>
            <GridData name="GridData" width="150"></GridData>
        </CheckEnableComposite>
    </Composite>
</QuickFunction>