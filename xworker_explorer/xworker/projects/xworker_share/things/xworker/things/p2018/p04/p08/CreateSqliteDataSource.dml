      Scode=
#$@text#$@
//表单数据
def values = thingForm.doAction("getValues", actionContext);

//向导正在编辑的事物
def thing = thingGuide.getEditThing();
thing.put("dbType", "sqlite");
thing.put("drivers", "15");
thing.put("driver_class", "org.sqlite.JDBC");
thing.put("url", "template:jdbc:sqlite:" + values.dbFile);
thing.put("userName", values.userName);
thing.put("password", values.password);

return true;
