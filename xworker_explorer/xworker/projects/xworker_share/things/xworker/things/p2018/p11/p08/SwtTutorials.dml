                Scode=
#$@text#$@
import org.eclipse.swt.widgets.Control;
import xworker.swt.design.Designer;

def thing = event.item.getData()._source;
def control = actionContext.get(thing.getMetadata().getName());
if(control instanceof Control){
    def oldControl = actionContext.get("oldControl");
    if(oldControl != null){
        Designer.unmarkControl(oldControl);
    }

    actionContext.g().put("oldControl", control);
    Designer.markControl(control);
}
#$@text#$@
