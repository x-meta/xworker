#$@text#$@
import org.xmeta.Thing;

def ac = parentContext;
def thing = ac.get("shellExample");
if(thing != null){
    return thing;
}

thing = new Thing("xworker.swt.widgets.Shell");
thing.set("name","shell");
ac.g().put("shellExample", thing);
return thing;
