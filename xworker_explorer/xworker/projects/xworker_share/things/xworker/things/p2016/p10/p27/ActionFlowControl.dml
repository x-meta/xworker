import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.MessageBox;

import xworker.util.XWorkerUtils;
import org.xmeta.util.UtilString;

def result = false;
def ide = XWorkerUtils.getIDEShell();
ide.display.syncExec({
    def box = new MessageBox(XWorkerUtils.getIDEShell(), SWT.ICON_INFORMATION | SWT.OK | SWT.CANCEL);
    box.setText(UtilString.getString("lang:d=是否Return&en=Return?", actionContext));
    box.setMessage(UtilString.getString("lang:d=点击确定测试Return，点击取消继续其他测试。&en=Click OK to test Return, click Cancel to continue the other tests.", actionContext));
    result = box.open() == SWT.OK;
});

return result;
