if(requestBean.input != null){
    println(requestBean.input);
    return "success";
}else if(requestBean.ac == "qrcode"){
    def requestUrl = request.getRequestURL() + "?sc=xworker.things.p2016.p12.p06.PrintInputToConsole/@SimpleControl";
    println(requestUrl);
    self.doAction("writeQrCode", actionContext, "message", requestUrl, "out", response.getOutputStream());
    return "qrcode";    
}else{
    return "success";
}
