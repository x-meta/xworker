def items = dataObjectContext.dataTable.getSelection();
if(items != null && items.length > 0){
    def item = items[0];
    def fileName = item.getData().get("FILE_NAME");
    def tablespaceName = item.getData().get("TABLESPACE_NAME");
    
    def sql = "--拷贝下面的代码，修改dataFile然后在数据库工具中执行\r\n";
    sql = sql+ "alter tablespace " + tablespaceName + " add datafile '" + fileName + "' size 1G autoextend on";
    
    //显示代码
    actions.doAction("showCode", actionContext, ["code":sql]);
}
