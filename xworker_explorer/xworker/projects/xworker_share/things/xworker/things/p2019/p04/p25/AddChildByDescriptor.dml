#$@text#$@
import org.xmeta.Thing;

//菜单定义
def menu = event.widget.getData("menu");

//新建子节点并保存
def desc = menu.params;
def thing = new Thing(desc);
def name = currentThing.getMetadata().getName() + thing.getThingName();
thing.set("name", name);
currentThing.addChild(thing);
currentThing.save();
