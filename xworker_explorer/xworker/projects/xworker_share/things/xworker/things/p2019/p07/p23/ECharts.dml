    sdisableGlobalContext=false
    Scode=
#$@text#$@
import xworker.dataObject.DataObject;

try{
    //开始批量修改
    datas.begin();
    add(datas, "Mon", 820);
    add(datas, "Tue", 932);
    add(datas, "Wed", 901);
    add(datas, "Thu", 934);
    add(datas, "Fri", 1290);
    add(datas, "Sat", 1330);
    add(datas, "Sun", 1320);
}finally{
    //结束批量修改
    datas.finish();
}

def add(datas, date, value){
    def data = new DataObject("xworker.things.p2019.p07.p23.ECharts/@Composite/@datas/@DataObjects/@AbstractDataObject");
    data.put("date", date);
    data.put("value", value);
    datas.add(data);
}
