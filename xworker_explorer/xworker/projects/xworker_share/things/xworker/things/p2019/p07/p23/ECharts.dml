    Sscript=
#$@text#$@
var option = {
    xAxis: {
        type: 'category',
        data: []
    },
    yAxis: {
        type: 'value'
    },
    series: [{
        data: [],
        type: 'line'
    }]
};

swtChart.setOption(option);
#$@text#$@
    Soption=
#$@text#$@
template:{
    xAxis: {
        data: [<#list records as r>'${r.date?if_exists}'<#if r_has_next>,</#if></#list>]
    },
    series: [{
        data: [<#list records as r>'${r.value?if_exists}'<#if r_has_next>,</#if></#list>]
    }]
}
