#$@text#$@
import xworker.dataObject.DataObject;

if(event.widget == insertButton){
    def r = new Random();
    def data = new DataObject("xworker.things.p2019.p07.p23.ECharts/@Composite/@datas/@DataObjects/@AbstractDataObject");
    data.put("date", "Date-" + System.currentTimeMillis());
    data.put("value", r.nextInt(10000));
    datas.add(data);
}else{
    if(table.getSelection().length == 0){
        return;
    }
    
    def data = table.getSelection()[0].getData();    
    datas.remove(data);
}
