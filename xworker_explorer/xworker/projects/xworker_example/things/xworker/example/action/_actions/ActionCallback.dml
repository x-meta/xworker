#$@text#$@
import org.xmeta.ActionException;

def file = self.doAction("getFile", actionContext);
if(file == null){
    throw new ActionException("File can't be null, action=" + self.metadata.path);
}

file.eachLine {
    //处理每一行
    self.doAction("handleLine", actionContext, ["line": it]);
}
