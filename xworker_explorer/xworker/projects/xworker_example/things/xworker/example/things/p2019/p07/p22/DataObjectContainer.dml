#$@text#$@
import xworker.dataObject.DataObject;

def colors = ["#00FF00","#FF8080"];
def icons = ["icons/accept.png", "icons/add.png", "icons/anchor.png", "icons/application.png"];

def r = new Random();

datas.begin();
try{
    for(int i=0; i<10; i++){
        def data = new DataObject("xworker.example.things.p2019.p07.p22.DataObjectContainer/@Composite/@datas/@DataObjects/@AbstractDataObject");
        data.put("name", "192.168.1." + (i + 1));
        data.put("status", colors.get(r.nextInt(colors.size())));
        data.put("image", icons.get(r.nextInt(icons.size())));
        datas.add(data);
    }
}finally{
    datas.finish();
}
