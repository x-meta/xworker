#$@text#$@
import xworker.dataObject.DataObject;

if(event.widget == insertButton){
    def data = new DataObject("xworker.example.things.p2019.p07.p22.DataObjectContainer/@Composite/@datas/@DataObjects/@AbstractDataObject");
    data.put("name", "192.168.1.10");
    data.put("status", "#00FF00");
    data.put("image", "icons/accept.png");
    datas.add(data);
}else{
    if(table.getSelection().length == 0){
        return;
    }
    
    def data = table.getSelection()[0].getData();    
    datas.remove(data);
}
