<QuickFunction name="HelloWorld21" descriptors="xworker.swt.xworker.ThingRegistThing" label="HelloWorld2"
     type="thingDemo" path="xworker.example.things.p2020.p08.p20.HelloWorld2" group="dynamicModel.example"
