#$@text#$@
def values = thingForm.doAction("getValues", actionContext);
def path = values.file;
if(path == null || path.trim() == ""){
    return false;
}

def file = new File(path);
if(file.exists() == false || file.isDirectory()){
    return false;
}

actionContext.l().put("file", path);
return true;
#$@text#$@
