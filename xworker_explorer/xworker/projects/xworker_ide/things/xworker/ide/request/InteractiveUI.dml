      Scode=
#$@text#$@
//原先的选择
def oldRequest = null;
if(requestTable.getSelectionCount() > 0){
    oldRequest = requestTable.getSelection()[0].getData();
}

//重新刷新
requestTableDataStore.doAction("reload", actionContext);

if(oldRequest != null){
    for(item in requestTable.getItems()){
        if(item.getData() == oldRequest){
            requestTable.setSelection(item);
            break;
        }
    }
}

foreFinishButton.setEnabled(false);
removeButton.setEnabled(false);
