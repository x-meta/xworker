#$@text#$@
def composite = null;

//先从属性获取
def path = self.getString("compositePath");
composite = world.getThing(path);

//再从子节点获取
if(composite == null){
    composite = self.getThing("Composite@0");
}

if(composite == null){
    //显示文档
    actions.doAction("setContent", actionContext, [content: self, type:"thingDoc", thing: self]);
}else{
    //显示Composite
    actions.doAction("setContent", actionContext, [content: composite, type:"composite", thing: self]);
}
