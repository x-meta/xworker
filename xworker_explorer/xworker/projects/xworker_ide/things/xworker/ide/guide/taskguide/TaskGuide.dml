import xworker.util.XWorkerUtils;
import org.xmeta.ActionContext;

def shell = XWorkerUtils.getIDEShell();
def ac = new ActionContext();
ac.put("parent", shell);
ac.put("thingPath", self.getMetadata().getPath());

def thing = world.getThing("xworker.ide.guide.taskguide.TaskGuideShell");
def newShell = thing.doAction("create", ac);

ac.actions.doAction("setContent", ac, [content: self, type:"thingDoc"]);
newShell.open();
