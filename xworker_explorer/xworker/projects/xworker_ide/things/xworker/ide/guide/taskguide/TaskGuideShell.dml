@/@composite/@actions/@changeContent
sname
setContent
sisSynchronized
false
sthrowException
true
suseOtherAction
false
svarScope
Local
sdisableGlobalContext
false
Scode
#$@text#$@
import org.xmeta.ActionContext;
import xworker.util.XWorkerUtils;

if(actionContext.get("content") == null || actionContext.get("type") == null){
    log.info("content or type is null");
    return;
}

//转化类型
if(type == "thingDoc"){
    type = "browser";
    content = XWorkerUtils.getThingDescUrl(content);
}

if(type == "browser"){
    //网页
    browser.scripts.doAction("openUrl", actionContext, ["name":"default", "url":content]);
    compositeStackLayout.topControl = browser.mainComposite;
    composite.layout();
}else if(type == "composite"){
    //Composite
    for(control in contentComposite.getChildren()){
         control.dispose();
    }
    
    def ac = new ActionContext();
    ac.put("parent", contentComposite);
    content.doAction("create", ac);
    contentComposite.layout();

    compositeStackLayout.topControl = contentComposite;
    composite.layout();    
}else if(type == "function"){
    //执行的是函数
    functionRunner.run(content);
    functionRunner.getComposite().layout();

    compositeStackLayout.topControl = functionRunner.getComposite();
    composite.layout();   
}else if(type == "commander"){
    //命令
    commander.setRootWord(content);
    commander.clear();
      
    compositeStackLayout.topControl = commander.getActionContext().get("mainComposite");
    composite.layout();         
}
#$@text#$@
