#$@text#$@
import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.TableItem;

if(fileName == null){
    return;
}

dirText.setText(fileName);

//清空表格
projectsTable.removeAll();

//查找项目
def projects = [];
findProjects(new File(fileName), projects);

//列出项目
for(prj in projects){
    def item = new TableItem(projectsTable, SWT.NONE);
    def name = prj.name;
    if(world.getThingManager(prj.name) != null){
        //name = name + "(已存在)";
        item.setForeground(redColor);
        prj.exists = true;
    }else{
        prj.exists = false;
    }
    item.setText([name, prj.file.getPath()] as String[]);    
    item.setData(prj);
    
    //如果项目已经存在，不可选
    if(world.getThingManager(prj.name) != null){
        item.setGrayed(true);
    }
}

def findProjects(file, projects){
    if(file.isDirectory()){
        //查看项目文件是否存在
        def pfile = new File(file, "xworker.properties");
        if(pfile.exists() == false){
            pfile = new File(file, "dml.prj");            
        }
        if(pfile.exists() == false){
            pfile = new File(file, ".dml");            
        }
        if(pfile.exists() == false){
            pfile = new File(file, "config.properties");            
        }
                
        if(pfile.exists()){
            //读取项目
            def prjName = getPrjName(pfile);
            //println prjName;
            if(prjName == null){
                prjName = file.getName();
            }
            
            def prj = [:];
            prj.name = prjName;
            prj.file = file;
            projects.add(prj);
            //println("xxxxxxxxxx");
        }else{
            //遍历子目录
            for(childFile in file.listFiles()){
                findProjects(childFile, projects);
            }
        }
    }
}

def getPrjName(pfile){
    def name = null;
    pfile.eachLine {
        if(it.trim() == ""){
            
        }else{                
            //println(it);
            def iss = it.split("[=]");
            if(iss.length == 2){
                if(iss[0] == "projectName"){
                     name = iss[1];
                     return name;
                }else if(iss[0] == "name"){                
                     name = iss[1];
                     return name;
                }
            }
        }
    }  
    
    return name;
}
