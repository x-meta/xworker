#$@text#$@
import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.MessageBox;

import xworker.util.XWorkerUtils;

//校验
if(thingForm.doAction("validate", actionContext)){
    def values = thingForm.doAction("getValues", actionContext);
    values.file = values.file + "/" + values.name;
    def file = new File(values.file);
    if(file.exists() && file.listFiles().length > 0){
        def shell = Composite.getShell();
        def box = new MessageBox(shell, SWT.ICON_WARNING | SWT.OK | SWT.CANCEL);
        box.setText("创建Eclipse项目");
        box.setMessage("文件夹" + values.file + "已存在且不为空，是否要覆盖？");
        if(box.open() == SWT.CANCEL){
            return null;
        }
    }
    
    if(values.type == "model"){
        actions.doAction("createDmlProject", actionContext, values);
    }else if(values.type == "java_maven"){
        actions.doAction("createMavenProject", actionContext, values);
    }else if(values.type == "java_min"){
        values.includeXWorkerLib = false;
        actions.doAction("createJavaProject", actionContext, values);
    }else{
        values.includeXWorkerLib = true;
        actions.doAction("createJavaProject", actionContext, values);
    }
    
    if(values.addToXWorker == true || values.addToXWorker == "true"){
        //println("adddddd");
        if(values.type == "model"){
            XWorkerUtils.addOuterProject(values.name, values.file);
            return world.initThingManager(new File(values.file));
        }else{
            XWorkerUtils.addOuterProject(values.name, values.file + "/things/");
            return world.initThingManager(new File(values.file + "/things/"));
        }
    }else{
        def shell = Composite.getShell();
        def box = new MessageBox(shell, SWT.ICON_WARNING | SWT.OK | SWT.CANCEL);
        box.setText("创建Eclipse项目");
        box.setMessage("Eclipse项目创建成功，要打开所在文件夹么？");
        if(box.open() == SWT.OK){
            actions.doAction("openDir", actionContext, "dir", values.file);
        }
        return true;
    }
}

return null;
