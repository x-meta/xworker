import java.sql.DriverManager;

import groovy.sql.Sql;

//校验
if(thingForm.doAction("validate", actionContext)){
    def values = thingForm.doAction("getValues", actionContext);
    
    def msg = world.getAction("xworker.ide.worldexplorer.swt.dialogs.NewProjectIndex/@createModelProject/@Composite/@actions/@showErrorMessage");
    //测试数据库    
    try{
        Class.forName(values.driverClass);
    }catch(Exception e){
        msg.run(actionContext, ["title": "Jdbc项目", "message": "装载驱动失败，" + e.getMessage()]);
        return null;
    }
    
    //测试数据库连接
    def con = null;
    try{
        con = DriverManager.getConnection(values.url, values.user, values.password);
    }catch(Exception e){
        msg.run(actionContext, ["title": "Jdbc项目", "message": "连接数据库失败，" + e.getMessage()]);
        return null;
    }
    
    //测试表是否存在
    def pst = null;
    try{
        pst = con.prepareStatement("select * from tblCategorys");
        def rs = pst.executeQuery();
        rs.close();
        pst.close();
    }catch(Exception e){
        msg.run(actionContext, ["title": "Jdbc项目", "message": "查询事物相关表失败，" + e.getMessage()]);
        return null;
    }finally{
        pst.close();
        con.close();
    }
    
    def file = null;
    def properties = "driverClass=" + values.driverClass + "\r\n" + 
            "url=" + values.url + "\r\n" + 
            "user=" + values.user + "\r\n" + 
            "password=" + values.password + "\r\n" + 
            "class=org.xmeta.thingManagers.JdbcThingManager\r\n";
            
    if(values.dir != null && values.dir != ""){
        //创建外部项目
        values.dir = values.dir + "/" + values.name;
        file = new File(values.dir);
        if(!file.exists()){
            file.mkdirs();
        }
        
        return create(actionContext, "name", values.name, "dir", file, "properties", properties);
    }else{
        //创建内部项目
        return create(actionContext, "name", values.name, "dir", null, "properties", properties);
    }
    /*
    def file = new File(world.getPath() + "/projects/" + values.name + "/xworker.properties");
    file.getParentFile().mkdirs();

    def printWriter = file.newPrintWriter() //         
    printWriter.write("projectName=" + values.name + "\r\n") ;
    printWriter.write("driverClass=" + values.driverClass + "\r\n");
    printWriter.write("url=" + values.url + "\r\n");
    printWriter.write("user=" + values.user + "\r\n");
    printWriter.write("password=" + values.password + "\r\n");
    printWriter.write("class=org.xmeta.thingManagers.JdbcThingManager\r\n");
    printWriter.flush()  
    printWriter.close()
   
    return world.initThingManager(file.getParentFile());
    */
}

