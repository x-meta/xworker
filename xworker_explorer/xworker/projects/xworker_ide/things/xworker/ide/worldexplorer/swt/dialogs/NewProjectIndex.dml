#$@text#$@
import org.xmeta.Thing;

//如果包含目录
file = new File(values.file);
if(file.exists() == false){
    if(actions.doAction("mkdirs", actionContext, "file", file) == false){
        return null;
    }
}else if(file.listFiles().length > 0){
    actions.doAction("showErrorMessage", actionContext, "title", "创建项目", "message", "目录不为空，请选择一个空的文件夹！");
    return null;
}
   
//是否包含things目录
def prjFile = null;
if(values.things == "true"){
    prjFile = new File(file, "xworker.properties");
    
    //创建things目录
    def thingsFile = new File(file, "things");
    thingsFile.mkdirs();
}else{
    prjFile = new File(file, "dml.prj");
}

//保存项目文件
def printWriter = prjFile.newPrintWriter(); //         
printWriter.write("projectName=" + values.name) ;
printWriter.flush()  
printWriter.close()

//创建SimpleThingManager
def simpleThingManager = new File(file, "SimpleThingEditor.dml");
printWriter = simpleThingManager.newPrintWriter(); //         
printWriter.write("""<?xml version="1.0" encoding="utf-8"?>

<thing name="SimpleThingEditor" descriptors="xworker.lang.MetaDescriptor3">
    <actions>
        <ThingAction1 name="run" thingPath="xworker.swt.xwidgets.prototypes.SimpleThingEditor" actionName="run"></ThingAction1>
    </actions>
</thing>""") ;
printWriter.flush()  
printWriter.close()

//添加到启动项目中
def projectSet = world.getThing("_local.xworker.worldExplorer.ProjectSet");
if(projectSet == null){
    projectSet = new Thing("xworker.ide.worldexplorer.things.ProjectSet");
    projectSet.saveAs("_local", "_local.xworker.worldExplorer.ProjectSet");
}
projectSet.doAction("addProject", actionContext, ["projectFile": file, "isWorkspace": false]);

//创建事物管理器
return world.getThingManager(values.name);
