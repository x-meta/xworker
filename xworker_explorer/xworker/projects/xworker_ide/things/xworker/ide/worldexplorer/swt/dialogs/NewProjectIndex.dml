    def file = null;
    def properties = "host=" + values.host + "\r\n" + 
            "port=" + values.port + "\r\n" + 
            "user=" + values.user + "\r\n" + 
            "password=" + values.password + "\r\n" + 
            "class=org.xmeta.thingManagers.RedisThingManager\r\n";
            
    if(values.dir != null && values.dir != ""){
        //创建外部项目
        values.dir = values.dir + "/" + values.name;
        file = new File(values.dir);
        if(!file.exists()){
