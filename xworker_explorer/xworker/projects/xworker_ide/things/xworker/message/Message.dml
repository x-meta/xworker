#$@text#$@
//获取类型
def type = self.getString("type");
if(type == null || "" == type){
    type = "thingDesc";
}

//根据类型获取相对应的值
switch(type){
    case "url":    
    case "textMessage":
    case "htmlMessage":
        _g.put("content", self.doAction("getString", actionContext));
        //println "textMessage";
        break;
    case "thingRegist":
        def content = self.doAction("getString", actionContext);
        def ps = content.split("[|]");
        if(ps.length < 2){
            return;
        }
        actionContext.g().put("registType", ps[0]);
        content = world.getThing(ps[1]);
        if(ps.length >= 3){
            actionContext.g().put("descriptor", ps[2]);
        }else{
            actionContext.g().put("descriptor", null);
        }
        break;
    case "groovy":
    case "code":
        _g.put("content", self);
        break;    
    case "thingDesc":
        _g.put("content", self.doAction("getThingSelf", actionContext));
        break;
    case "thingControl":
    case "thingEditor":
    case "composite":
    case "shell":
    case "uiFlow":
    case "autoDemo":
    case "swtGuide":
    case "webDemo":
    case "thingDemo":
    case "action":
    case "dataObjectEdtior":
    case "dataObjectSelector":
    case "dataObjectMultiSelector":    
        _g.put("content", self.doAction("getThing", actionContext));
        break;
}
