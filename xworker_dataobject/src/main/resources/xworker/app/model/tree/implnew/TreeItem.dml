<?xml version="1.0" encoding="utf-8"?>

<thing name="TreeItem" descriptors="xworker.lang.MetaDescriptor3" extends="xworker.app.model.tree.SwtScripts">
    <description><![CDATA[<p>树模型的树节点。</p>

<p>&nbsp;</p>
]]></description>
    <attribute name="name" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="" group="TreeNode" validateOnBlur="false" allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="label" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         description="&lt;p&gt;显示在Tree上的文字是text属性，label属性是显示在事物编辑中的标签。&lt;/p&gt;" group="TreeNode"
         validateOnBlur="false" allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="tabId" group="TreeNode" labelAlign="" labelVAlign="" description="&lt;p&gt;树常用于应用程序的导航树，点击后在编辑器的TabFolder等打开对应的标签页，而tabId可以用于标签页的标识。&lt;/p&gt;"></attribute>
    <attribute name="dataId" group="TreeNode" labelAlign="" labelVAlign="" description="&lt;p&gt;不同于树节点的标识，dataId常用来表示树节点所代表的数据的真实标识。&lt;/p&gt;"></attribute>
    <attribute name="url" size="60" colspan="2" group="TreeNode" showLabel="" labelAlign="" labelVAlign=""
         modifier="" description="&lt;p&gt;在ExtJs中如果树用于导航菜单等，url指向要打开的编辑器（do?sc=RemoteWidgets）。&lt;/p&gt;"></attribute>
    <attribute name="id" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="" group="TreeNode" validateOnBlur="false" allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="cls" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="" description="&lt;p&gt;样式单的类。&lt;/p&gt;" group="TreeNode" validateOnBlur="false" allowDecimals="false"
         allowNegative="false"></attribute>
    <attribute name="leaf" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="false" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false">
        <description><![CDATA[<p>是否是根节点，如果不是根节点且没有子节点，可能会调用TreeModel.getChilds方法动态刷新子节点。</p>
<p>如果树节点是Thing且children@不为空，那么是不会动态刷新子节点的。</p>]]></description>
    </attribute>
    <attribute name="href" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="" default="#" size="60" colspan="2" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="allowChildren" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="true" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="allowDrag" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="true" inputtype="truefalse" group="dragdrop" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="allowDrop" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="true" inputtype="truefalse" group="dragdrop" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="checked" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="false" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="disabled" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="false" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="draggable" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="false" inputtype="truefalse" group="dragdrop" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="editable" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="true" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="expandable" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="false" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="expanded" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="false" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="hidden" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="false" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="hrefTarget" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="" group="TreeNode" validateOnBlur="false" allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="icon" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="" inputtype="imageSelector" size="50" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="iconCls" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="" group="TreeNode" validateOnBlur="false" allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="isTarget" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="true" inputtype="truefalse" group="dragdrop" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="qtip" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="" group="TreeNode" validateOnBlur="false" allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="singleClickExpand" descriptors="xworker.dataObject.thing.ThingDataObject/@attribute"
         type="boolean" default="false" inputtype="truefalse" group="TreeNode" validateOnBlur="false"
         allowDecimals="false" allowNegative="false"></attribute>
    <attribute name="fontName" group="TreeNode"></attribute>
    <attribute name="fontSize" group="TreeNode"></attribute>
    <attribute name="fontStyle" group="TreeNode">
        <value name="BOLD" value="BOLD"></value>
        <value name="ITALIC" value="ITALIC"></value>
        <value name="BOLD_ITALIC" value="BOLD_ITALIC"></value>
    </attribute>
    <thing name="TreeItem" extends="xworker.app.model.tree.implnew.TreeItem" inheritDescription="true"></thing>
    <thing name="TreeModelItem" extends="xworker.app.model.tree.TreeModels">
        <description><![CDATA[<p>使用树模型来构建树节点，如果树模型显示根节点，那么返回根节点，否则把根节点下的一级子节点全部加入到父节点中。</p>

<p>&nbsp;</p>

<p>&nbsp;</p>
]]></description>
        <attribute name="name"></attribute>
        <attribute name="label"></attribute>
        <attribute name="description" inputtype="html"></attribute>
    </thing>
</thing>