<?xml version="1.0" encoding="utf-8"?>

<thing name="H" descriptors="xworker.lang.MetaDescriptor3">
    <actions>
        <GroovyAction name="toHtml" code="return &quot;&quot;&quot;&lt;h${self.get(&quot;level&quot;)}&gt;${self.get(&quot;text&quot;)}&lt;/h${self.get(&quot;level&quot;)}&gt;&quot;&quot;&quot;;"></GroovyAction>
    </actions>
    <attribute name="name"></attribute>
    <attribute name="label"></attribute>
    <attribute name="level" inputtype="select" default="1">
        <value name="1" value="1"></value>
        <value name="2" value="2"></value>
        <value name="3" value="3"></value>
        <value name="4" value="4"></value>
        <value name="5" value="5"></value>
        <value name="6" value="6"></value>
    </attribute>
    <attribute name="text" inputtype="textarea"></attribute>
    <attribute name="description" inputtype="html"></attribute>
</thing>