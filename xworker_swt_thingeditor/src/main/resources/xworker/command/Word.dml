import org.xmeta.Thing;
import org.xmeta.ActionContext;
import xworker.util.XWorkerUtils;


//使用新的变量上下文创建窗口
def ac = new ActionContext();
ac.put("parent", XWorkerUtils.getIDEShell());
 
def thing = world.getThing("xworker.command.Commander");
commanderShell = thing.doAction("create", ac); 
ac.actions.doAction("putWord", ac, ["word": currentThing]);
commanderShell.open();
