#$@text#$@
import org.xmeta.ActionContext;

import org.eclipse.swt.SWT;

if((event.keyCode == 'C' || event.keyCode == 'c') && event.stateMask == SWT.ALT){
    //ALT + c重置
    commander.clear();
}else if((event.keyCode == 'V' || event.keyCode == 'v') && event.stateMask == SWT.ALT){
    //查看当前语句
    def ac = new ActionContext();
    ac.put("parent", commandText.getShell());
    ac.put("sentence", commander.getCurrentSentence());
    def thing = world.getThing("xworker.command.SentenceViewer");
    def shell = thing.doAction("create", ac);
    shell.open();
}else if(event.keyCode == 8){
    commander.removeLastWord();
}else{
    commander.handleKeyEvent(event);
}
