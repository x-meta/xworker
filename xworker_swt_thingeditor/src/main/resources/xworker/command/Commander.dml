import xworker.swt.util.SwtDialog;
import org.xmeta.ActionContext;
import org.xmeta.Thing;

def words = commander.getCurrentSentence().words;
def thing = null;
for(int i=words.size() -1 ; i>=0; i--){
    if(words.get(i) instanceof Thing){
        thing = words.get(i);
        break;
    }
}

if(thing == null){
    return false;
}


//获取索引事物
def ac = new ActionContext();
ac.put("parent", addButton.getShell());
ac.put("thing", thing);
def dthing = world.getThing("xworker.swt.xworker.prototype.ThingRegistIndexShell");
def shell = dthing.doAction("create", ac);

def indexPath = SwtDialog.open(shell, ac);
if(indexPath == null){
    return false;
}else{
    actionContext.l().put("indexPath", indexPath);
    return true;
}
