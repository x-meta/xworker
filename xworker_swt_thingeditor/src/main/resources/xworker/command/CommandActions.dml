#$@text#$@
import xworker.command.Selector;

import org.xmeta.ActionContext;

if(!commander.checkIndexDiff(words, index, self)){
    return;
}

//创建选择面板
def descritporForNewThing = self.getStringBlankAsNull("descritporForNewThing");
if(descritporForNewThing == null){
    descritporForNewThing = "xworker.command.Word";
}
def compositeThing = world.getThing("xworker.command.selectors.RegistSelectorShell/@mainComposite");
def composite = compositeThing.doAction("create", actionContext);

//初始化注册选择
def thing = world.getThing(self.doAction("getThingPath", actionContext));
def type = self.getStringBlankAsNull("type");
if(type == null){
    type = "command";
}
//println(type + ":" + thing);
//registSelector是compositeThing创建的变量
registSelector.doAction("init", actionContext, ["thing": thing, "type":type, "keywords":"", "autoLoad":true]);
registSelector.getActionContext().g().put("descritporForNewThing", descritporForNewThing);
    
return new Selector(actionContext, actions);
