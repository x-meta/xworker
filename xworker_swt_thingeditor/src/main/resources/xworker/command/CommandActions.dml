#$@text#$@
import xworker.command.Selector;
import xworker.util.XWorkerUtils;

if(!commander.checkIndexDiff(words, index, self)){
    return;
}

//设置初始化变量
def g = actionContext.g();
g.put("configPath", self.getString("configPath"));
g.put("configDescriptor", self.getString("configDescriptor"));
g.put("thingManager", self.getString("configThingManager"));

//创建选择面板
def compositeThing = world.getThing("xworker.command.selectors.LanuchProgramSelector/@Composite");
def composite = compositeThing.doAction("create", actionContext);
def actions = null;
def actionContainerName = self.getStringBlankAsNull("actionContainerName");
if(actionContainerName != null){
    actions = actionContext.get(actionContainerName);
}
    
return new Selector(actionContext, actions);
