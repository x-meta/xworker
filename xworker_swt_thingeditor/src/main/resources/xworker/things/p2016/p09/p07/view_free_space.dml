      skeepQuerySql=false
      SquerySql=
#$@text#$@
select a.tablespace_name ,count(1) 碎片量 from
dba_free_space a, dba_tablespaces b 
where a.tablespace_name =b.tablespace_name
and b.contents not in('TEMPORARY','UNDO','SYSAUX')
group by a.tablespace_name
having count(1) >20
order by 2
#$@text#$@
