#$@text#$@
import java.net.Socket;

def values = thingForm.doAction("getValues", actionContext);

try{
    def ss = new Socket(values.host, Integer.parseInt(values.port));
    ss.close();
    
    text.setText("连接" + values.host + ":" + values.port + "成功！");
}catch(Exception e){
    text.setText("连接" + values.host + ":" + values.port + "失败！\n" + e.getMessage());
}
