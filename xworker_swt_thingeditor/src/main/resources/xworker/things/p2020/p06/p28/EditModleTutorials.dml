    <Guide name="init" label="初始化" en_label="Init">&#xd;
        <actions>&#xd;
            <GroovyAction name="init">&#xd;
                <code><![CDATA[import org.xmeta.Thing;
import xworker.util.XWorkerUtils;

//新建事物并打开编辑
