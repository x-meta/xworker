#$@text#$@
import org.xmeta.Thing;

if(!form.doAction("validate", actionContext)){
    return;
}

//获取表单的值
def values = form.doAction("getValues", actionContext);

def thing = new Thing("xworker.lang.actions.ExecuteXWorkerProcess");
thing.getAttributes().putAll(values);

thing.doAction("run", actionContext);
