#$@text#$@
import org.eclipse.swt.widgets.Shell;
import org.eclipse.swt.SWT;

//设置表单内容
//println mainComposite.getSize();
def ui = parentContext.parentContext.ui;

thingForm.doAction("setValues", actionContext, "values", ui.getThing().getAttributes());

if(ui.getControl() instanceof Shell){
    def parentThing = ui.getControl().getData("thing");
    parentThingForm.doAction("setThing", actionContext, "thing", parentThing);
}else{
    shellGroup.dispose();
}

mainComposite.layout();
mainComposite.setSize(mainComposite.computeSize(SWT.DEFAULT, SWT.DEFAULT));
mainScrollComposite.setMinSize(mainComposite.getSize());
#$@text#$@
