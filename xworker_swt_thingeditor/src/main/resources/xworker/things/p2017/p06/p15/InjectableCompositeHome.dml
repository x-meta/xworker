#$@text#$@
import xworker.command.interactive.InteractiveUI;

//保存值到事物
def values = parentThingForm.doAction("getValues", actionContext);

//设置表单内容
def ui = parentContext.parentContext.ui;
def uiThing = ui.getControl().getData("thing");

uiThing.getAttributes().putAll(values);
uiThing.save();

//重置
def uiActionContext = ui.getActionContext();
ui.getControl().dispose();
def shell = uiThing.doAction("create", uiActionContext);
shell.setVisible(true);

//重新连UI
def newUI = shell.getData(InteractiveUI.DATA_KEY);
newUI.connect();
#$@text#$@
