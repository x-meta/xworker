import org.xmeta.codes.XmlCoder;
import org.xmeta.Thing;

try{
    def xml = codeText.getText();
    def thing = new Thing();
    XmlCoder.parse(thing, xml);
    
    return thing;
}catch(Exception e){
    msg(actionContext, "message", e.getMessage());
    return null;
}
