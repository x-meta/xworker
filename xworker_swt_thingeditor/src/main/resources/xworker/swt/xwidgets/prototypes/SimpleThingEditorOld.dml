import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.MessageBox;
import org.apache.commons.io.FileUtils;

def dir = null;
def s = menuTree.getSelection();
def treeItem = null;
if(s != null && s.length > 0){
    treeItem = s[0];
    s = treeItem.getData().get("dataId");
    dir = new File(s);
}

//没有选择文件
if(dir == null){
    noFileMsg(actionContext);
    return;
}

//确认删除
def msg = null;
if(dir.isDirectory()){
    msg = "确定要删除目录" + dir.getPath() + "及目录下的所有文件和子目录么！";
}else{
    msg = "确定要删除文件" + dir.getPath() + "么！";
}
delete(actionContext, "msg", msg, "treeItem", treeItem, "dir", dir);
