#$@text#$@
import java.util.Locale;

import org.xmeta.ActionContext;
import org.xmeta.util.UtilResource;
import org.xmeta.ui.session.SessionManager;

import xworker.util.XWorkerUtils;
import org.eclipse.swt.SWT;
import org.eclipse.swt.widgets.Display;
import org.eclipse.swt.widgets.Shell;

//当有编辑器未保存时，不修改语言
if(workbench.getEditorContainer() != null && workbench.getEditorContainer().isDirty()){
    saveDialog(actionContext);
    return;
}

boolean zh = true;

def session = SessionManager.getSession(null);
def locale = session.getLocale();
if(locale != null && !locale.getLanguage().equals(new Locale("en").getLanguage())){
    zh = true;
}else{
    zh = false;
}

if(zh == true){
    Locale l = new Locale("en", "US");
    session.setLocale(l);
    session.setI18nResource(UtilResource.getInstance(l));
}else{
    Locale l = new Locale("zh", "CN");
    session.setLocale(l);
    session.setI18nResource(UtilResource.getInstance(l));
}

//重新启动Workbench
def thing =  workbench.getThing();

//为避免系统退出，先建一个shell，保证不会触发最后一个Shell退出时系统退出
def shell = new Shell(SWT.NONE);
def oldDisplay = shell.getDisplay();
//关闭之前的
workbench.exit();
shell.dispose();
try{
    if(oldDisplay.getShells().length == 0){
        oldDisplay.dispose();
    }
}catch(Exception e){
}

//重新创建一个Workbench
def ac = new ActionContext();
ac.put("parent", new Display());
def newShell = thing.doAction("run", ac);
