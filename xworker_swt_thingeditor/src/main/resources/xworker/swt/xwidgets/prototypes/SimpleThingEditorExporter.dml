#$@text#$@
def thing = null;
def type = typeList.getSelection()[0];

if(type == "ExecuteableJar"){
    thing = world.getThing("xworker.swt.xwidgets.prototypes.SimpleThingEditorExporter/@exportGuide/@Guide/@ExecuteableJar");
}else if(type == "War"){
    thing = world.getThing("xworker.swt.xwidgets.prototypes.SimpleThingEditorExporter/@exportGuide/@Guide/@exportWar");
}else{
    thing = world.getThing("xworker.swt.xwidgets.prototypes.SimpleThingEditorExporter/@exportGuide/@Guide/@exportWar1");
}

if(thing != null){
    guide.go(thing);
}else{
    println "next thing is null";
}
