#$@text#$@
if(thingForm.doAction("validate", actionContext)){
    def values = thingForm.doAction("getValues", actionContext);
    actionContext.peek().put("jarFile", values.jarFile);
    
    def type = values.type;
    if(type == "includeJettyServer"){
        def port = values.port;
        def contextPath = values.ContextPath;
       
        exportRapWarWithServer(actionContext, actionContext, "port", port, "contextPath", contextPath);
    }else if(type == "standlone"){   
        exportStandalone(actionContext);
    }else{
        export.run(actionContext);
    }
    
    exportButton.getShell().dispose();
}
