#$@text#$@
if(thingForm.doAction("validate", actionContext)){
    def values = thingForm.doAction("getValues", actionContext);
    actionContext.peek().put("thing", values.thing);
    actionContext.peek().put("jarFile", values.jarFile);
    actionContext.peek().put("action", values.action);
    
    export.run(actionContext);
    exportButton.getShell().dispose();
}
